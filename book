			end
		end
		AFIREBALL:remove()
	end))
	DFIREBALL.Fire1.Enabled = true
	DFIREBALL.Fire2.Enabled = true
	DFIREBALL.Fire3.Enabled = true
	GYRO:remove()
	ATTACK = false
end

function PILLAR()
	ATTACK = true
	local SLAMMED = false
	local GYRO = IT("BodyGyro", RootPart)
	GYRO.D = 15
	GYRO.P = 2000
	GYRO.MaxTorque = VT(0, 4000000, 0)
	coroutine.resume(coroutine.create(function()
		BOOK.Handle.V.Color = Color3.new(1,0,0)
		BOOK.Handle.V.Material = "Neon"
		Swait(15)
		BOOK.Handle.V.Color = Color3.new(0,0,0)
		BOOK.Handle.V.Material = "Ice"
	end))
	local YOURDOOM = script.Pillar:Clone()
	YOURDOOM.Parent = Effects
	coroutine.resume(coroutine.create(function()
		repeat 
			Swait()
			CameraShake(Mouse.Hit.p,1e9,10,60)
			WACKYEFFECT({
					Time = 45,
					EffectType = "Sphere",
					Size = VT(100, .15, 100),
					Size2 = VT(100, .15, 100)/2.5,
					Transparency = 0,
					Transparency2 = 1,
					CFrame = CF(Mouse.Hit.p) * CFrame.new(0,0,0),
					MoveToPos = nil,
					RotationX = 0,
					RotationY = MRANDOM(-360,360),
					RotationZ = 0,
					Material = "Neon",
					Color = C3(.45,.45,.45),
					SoundID = 383953627,
					SoundPitch = MRANDOM(1.35,1.65),
					SoundVolume = 5,
					UseBoomerangMath = false,
					Boomerang = 10,
					SizeBoomerang = 9
				})
			Swait(55)
		until SLAMMED == true 
	end))
	for i=0, 3.5, 0.1 / Animation_Speed do
		Swait()
		YOURDOOM.Handle.CFrame = CF(Mouse.Hit.p) * CFrame.new(0,200,0) * ANGLES(RAD(0), RAD(SINE * 3), RAD(0))
		GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 3 + 2 * COS(SINE / 55)) * ANGLES(RAD(-10), RAD(-5), RAD(0)) * ANGLES(RAD(2 - 0.4 * COS(SINE / 32)), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-10), RAD(10 + 4 * COS(SINE / 45)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
  		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.15, -0.15, -.75) * ANGLES(RAD(120 - 5 * COS(SINE / 38)), RAD(35 - 5 * COS(SINE / 35)), RAD(7.5 - 5 * COS(SINE / 32)))* RIGHTSHOULDERC0, 0.15 / 3)
        LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, -.45) *ANGLES(RAD(150),RAD(50 + 10 * COS(SINE / 34)),RAD(-20 - 10 * COS(SINE / 34))) * LEFTSHOULDERC0, 0.15 / Animation_Speed)
  		RightHip.C0 = Clerp(RightHip.C0, CF(1, -.9 - .25 * COS(SINE / 22), -0.35) * ANGLES(RAD(-5 + 4 * COS(SINE / 34)), RAD(70 + 20 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)
        LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -.75 + .25 * COS(SINE / 22), -0.7) * ANGLES(RAD(-20 + 10 * COS(SINE / 32)), RAD(-80 - 10 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)		
	end
	for i=0, .5, 0.1 / Animation_Speed do
		Swait()
		YOURDOOM.Handle.CFrame = CF(Mouse.Hit.p) * CFrame.new(0,200,0) * ANGLES(RAD(0), RAD(SINE * 3), RAD(0))
		GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 3 + 2 * COS(SINE / 55)) * ANGLES(RAD(10), RAD(-5), RAD(0)) * ANGLES(RAD(2 - 0.4 * COS(SINE / 32)), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(10 + 4 * COS(SINE / 45)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
  		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.15, -0.15, -.75) * ANGLES(RAD(120 - 5 * COS(SINE / 38)), RAD(35 - 5 * COS(SINE / 35)), RAD(7.5 - 5 * COS(SINE / 32)))* RIGHTSHOULDERC0, 0.15 / 3)
        LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, -.45) *ANGLES(RAD(90),RAD(50 + 10 * COS(SINE / 34)),RAD(-20 - 10 * COS(SINE / 34))) * LEFTSHOULDERC0, 0.15 / Animation_Speed)
  		RightHip.C0 = Clerp(RightHip.C0, CF(1, -.9 - .25 * COS(SINE / 22), -0.35) * ANGLES(RAD(-5 + 4 * COS(SINE / 34)), RAD(70 + 20 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)
        LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -.75 + .25 * COS(SINE / 22), -0.7) * ANGLES(RAD(-20 + 10 * COS(SINE / 32)), RAD(-80 - 10 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)		
	end
	for i=0, .5, 0.1 / Animation_Speed do
		Swait()
		YOURDOOM.Handle.CFrame = CF(Mouse.Hit.p) * CFrame.new(0,200,0) * ANGLES(RAD(0), RAD(SINE * 3), RAD(0))
		GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 3 + 2 * COS(SINE / 55)) * ANGLES(RAD(30), RAD(10), RAD(0)) * ANGLES(RAD(2 - 0.4 * COS(SINE / 32)), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-10), RAD(10 + 4 * COS(SINE / 45)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
  		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.15, -0.15, -.75) * ANGLES(RAD(120 - 5 * COS(SINE / 38)), RAD(35 - 5 * COS(SINE / 35)), RAD(7.5 - 5 * COS(SINE / 32)))* RIGHTSHOULDERC0, 0.15 / 3)
        LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, -.45) *ANGLES(RAD(90),RAD(50 + 10 * COS(SINE / 34)),RAD(20 - 10 * COS(SINE / 34))) * LEFTSHOULDERC0, 0.15 / Animation_Speed)
  		RightHip.C0 = Clerp(RightHip.C0, CF(1, -.9 - .25 * COS(SINE / 22), -0.35) * ANGLES(RAD(-5 + 4 * COS(SINE / 34)), RAD(70 + 20 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)
        LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -.75 + .25 * COS(SINE / 22), -0.7) * ANGLES(RAD(-20 + 10 * COS(SINE / 32)), RAD(-80 - 10 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)		
	end
	CreateSound("905731845",STUFF,5,1.35,false)
	CameraShake(RootPart.Position,1e9,10,40)
	--FALLING
	local FINISHED = false
	coroutine.resume(coroutine.create(function()
		for i = 1, 35 do
			Swait()
			WACKYEFFECT({Time = 65, EffectType = "Wave", Size = VT(1,1,1)*7.5, Size2 = VT(15,1,15)*8.5, Transparency = .75, Transparency2 = 1, CFrame = CF(YOURDOOM.Handle.Bottom.Position)* CFrame.new(0,-2,0)*CFrame.Angles(RAD(0),RAD(MRANDOM(-180, 180)),RAD(0)), RotationX = 0, RotationY = math.random(-15,15), RotationZ = 0, Material = "Neon", Color = Color3.new(.45,.45,.45), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 20, SizeBoomerang = 20})   
			YOURDOOM.Handle.CFrame = YOURDOOM.Handle.CFrame * CFrame.new(0,-5.5,0) * ANGLES(RAD(0), RAD(SINE * 3), RAD(0))
		end
		CreateSound("178452217",STUFF,7,1.15,false)
		CreateSound("421327488",STUFF,8,1.25,false)
		CreateSound("5306343910",STUFF,8,1.15,false)
		CameraShake(YOURDOOM.Handle.Position,1e9,20,100)
		WACKYEFFECT({Time = 65, EffectType = "Wave", Size = VT(1,1,1)*7.5, Size2 = VT(15,1,15)*8.5, Transparency = .75, Transparency2 = 1, CFrame = CF(YOURDOOM.Handle.Bottom.Position)* CFrame.new(0,-2,0)*CFrame.Angles(RAD(0),RAD(MRANDOM(-180, 180)),RAD(0)), RotationX = 0, RotationY = math.random(-15,15), RotationZ = 0, Material = "Neon", Color = Color3.new(.45,.45,.45), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 20, SizeBoomerang = 20})   
		WACKYEFFECT({
				Time = 65,
				EffectType = "Sphere",
				Size = VT(100, .15, 100),
				Size2 = VT(100, .15, 100)*1.5,
				Transparency = 0,
				Transparency2 = 1,
				CFrame = CF(YOURDOOM.Handle.Bottom.Position) * CFrame.new(0,-2.5,0),
				MoveToPos = nil,
				RotationX = 0,
				RotationY = MRANDOM(-360,360),
				RotationZ = 0,
				Material = "Neon",
				Color = C3(.55,.55,.55),
				SoundID = nil,
				SoundPitch = MRANDOM(1.35,1.65),
				SoundVolume = 5,
				UseBoomerangMath = false,
				Boomerang = 10,
				SizeBoomerang = 9
		})
		WACKYEFFECT({Time = 65, EffectType = "Wave", Size = VT(1,.55,1)*7.5, Size2 = VT(15,1,15)*8.5, Transparency = .55, Transparency2 = 1, CFrame = YOURDOOM.Handle.Bottom.CFrame*CFrame.new(0,0,0)*CFrame.Angles(RAD(0),RAD(0),RAD(0)), MoveToPos = nil, RotationX = MRANDOM(-360,360), RotationY = MRANDOM(-360,360), RotationZ = MRANDOM(-360,360), Material = "Neon", Color = Color3.new(.45,.45,.45), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UserBoomerangMath = true, Boomerang = 100, SizeBoomerang = 90})
		WACKYEFFECT({Time = 55, EffectType = "Wave", Size = VT(1,.55,1)*7.5, Size2 = VT(15,1,15)*8.5, Transparency = .55, Transparency2 = 1, CFrame = YOURDOOM.Handle.Bottom.CFrame*CFrame.new(0,0,0)*CFrame.Angles(RAD(0),RAD(0),RAD(0)), MoveToPos = nil, RotationX = MRANDOM(-360,360), RotationY = MRANDOM(-360,360), RotationZ = MRANDOM(-360,360), Material = "Neon", Color = Color3.new(.45,.45,.45), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UserBoomerangMath = true, Boomerang = 100, SizeBoomerang = 90})
		WACKYEFFECT({Time = 65, EffectType = "Wave", Size = VT(1,.55,1)*7.5, Size2 = VT(15,1,15)*8.5, Transparency = .55, Transparency2 = 1, CFrame = YOURDOOM.Handle.Bottom.CFrame*CFrame.new(0,0,0)*CFrame.Angles(RAD(0),RAD(0),RAD(0)), MoveToPos = nil, RotationX = MRANDOM(-360,360), RotationY = MRANDOM(-360,360), RotationZ = MRANDOM(-360,360), Material = "Neon", Color = Color3.new(.45,.45,.45), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UserBoomerangMath = true, Boomerang = 100, SizeBoomerang = 90})
		WACKYEFFECT({Time = 45, EffectType = "Wave", Size = VT(1,.55,1)*7.5, Size2 = VT(15,1,15)*8.5, Transparency = .55, Transparency2 = 1, CFrame = YOURDOOM.Handle.Bottom.CFrame*CFrame.new(0,0,0)*CFrame.Angles(RAD(0),RAD(0),RAD(0)), MoveToPos = nil, RotationX = MRANDOM(-360,360), RotationY = MRANDOM(-360,360), RotationZ = MRANDOM(-360,360), Material = "Neon", Color = Color3.new(.45,.45,.45), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UserBoomerangMath = true, Boomerang = 100, SizeBoomerang = 90})
		AOETime(YOURDOOM.Handle.Bottom.Position,75)
		Swait(125)
		for i = 1,65 do 
			Swait()
			YOURDOOM.Handle.CFrame = YOURDOOM.Handle.CFrame * CFrame.new(0,5.5,0) * ANGLES(RAD(0), RAD(SINE / 3), RAD(0))
		end
		YOURDOOM:remove()
	end))
	SLAMMED = true
	GYRO:remove()
	ATTACK = false
end

local HSV = Color3.fromHSV
local Hue = 0
local Brightness = 0.5
local RainbowColor = HSV(0, 0, 0)
local Increment = 3
game:GetService("RunService").Stepped:Connect(function()
    Hue = Hue + Increment if Hue >= 360 then
        Hue = 0 
    end 
    RainbowColor = HSV(Hue / 360, Brightness, Brightness)
end)


function EYES()
	ATTACK = true
	local SHOOT = false
	local GYRO = IT("BodyGyro", RootPart)
	GYRO.D = 15
	GYRO.P = 2000
	GYRO.MaxTorque = VT(0, 4000000, 0)
	coroutine.resume(coroutine.create(function()
		BOOK.Handle.B.Color = Color3.new(1,0,0)
		BOOK.Handle.B.Material = "Neon"
		Swait(15)
		BOOK.Handle.B.Color = Color3.new(0,0,0)
		BOOK.Handle.B.Material = "Ice"
	end))
	local EYEBIG = script.Eyes:Clone()
	EYEBIG.Parent = Effects
	EYEBIG.Handle.CFrame = CF(RootPart.Position, Mouse.Hit.p) * CFrame.new(0,20 + 2 * COS(SINE/21),0) * ANGLES(RAD(0), RAD(90 + 5 * COS(SINE/21)), RAD(-25))
	CreateSound("1842482566",EYEBIG.Handle,5,3.65,false)
	coroutine.resume(coroutine.create(function()
		repeat 
			Swait()
			CameraShake(RootPart.Position,1e9,6,50)
	WACKYEFFECT({
			Time = 55,
