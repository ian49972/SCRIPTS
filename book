function Nullify()
	ATTACK = true
	Song = 0
	coroutine.resume(coroutine.create(function()
		BOOK.Handle.X.Color = Color3.new(1,0,0)
		BOOK.Handle.X.Material = "Neon"
		Swait(15)
		BOOK.Handle.X.Color = Color3.new(0,0,0)
		BOOK.Handle.X.Material = "Ice"
	end))
	NewHead.MainHead.RightEye.Fire1.Enabled = false
	NewHead.MainHead.RightEye.Fire2.Enabled = false
	NewHead.MainHead.RightEye.Fire3.Enabled = false
	NewHead.MainHead.LeftEye.Fire1.Enabled = false
	NewHead.MainHead.LeftEye.Fire2.Enabled = false
	NewHead.MainHead.LeftEye.Fire3.Enabled = false
	Head.Transparency = 1
	Head.CanCollide = false
	Torso.Transparency = 1
	Torso.CanCollide = false
	RightArm.Transparency = 1
	RightArm.CanCollide = false
	LeftArm.Transparency = 1
	LeftArm.CanCollide = false
	RightLeg.Transparency = 1
	LeftLeg.CanCollide = false
	LeftLeg.Transparency = 1
	for _, c in pairs(Character:GetChildren()) do
			if c and c.Parent and c.ClassName == "Accessory" then
				c.Handle.Transparency = 1
			end
	end
	BOOK.Parent = nil
	--[[FB1.Parent = nil
	FB2.Parent = nil
	FB3.Parent = nil
	FB4.Parent = nil
	FB5.Parent = nil
	FB6.Parent = nil]]
	DFIREBALL.Fire1.Enabled = false
	DFIREBALL.Fire2.Enabled = false
	DFIREBALL.Fire3.Enabled = false
	CameraShake(RootPart.Position,1e9,30,200)
	CreateSound("1842432824",STUFF,10,1,false)
