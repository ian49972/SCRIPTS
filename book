	CreateSound("1842430127",RootPart,10,1,false)
	CreateSound("1842430127",RootPart,10,.9,false)
	CreateSound("1842430127",RootPart,10,.85,false)
	NewHead.MainHead.RightEye.Fire1.Enabled = false
	NewHead.MainHead.RightEye.Fire2.Enabled = false
	NewHead.MainHead.RightEye.Fire3.Enabled = false
