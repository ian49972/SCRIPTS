	end))
	for i=0, 4, 0.1 / Animation_Speed do
		Swait()
		WACKYEFFECT({Time = 12, EffectType = "Wave", Size = VT(1,.75,1), Size2 = VT(15,1,15)*1.5, Transparency = .65, Transparency2 = 1, CFrame = EYEBIG.Handle.DivineEye.S2.CFrame*CFrame.new(0,-0.5,0)*CFrame.Angles(RAD(MRANDOM(-360, 360)),RAD(MRANDOM(-360, 360)),RAD(MRANDOM(-360, 360))), MoveToPos = nil, RotationX = MRANDOM(-360,360), RotationY = MRANDOM(-360,360), RotationZ = MRANDOM(-360,360), Material = "Neon", Color = Color3.new(.45,.85,.45), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UserBoomerangMath = true, Boomerang = 100, SizeBoomerang = 90})
