  		RightHip.C0 = Clerp(RightHip.C0, CF(1, -.9 - .25 * COS(SINE / 22), -0.35) * ANGLES(RAD(-5 + 4 * COS(SINE / 34)), RAD(70 + 20 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)
        LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -.75 + .25 * COS(SINE / 22), -0.7) * ANGLES(RAD(-20 + 10 * COS(SINE / 32)), RAD(-80 - 10 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)		
	end
	local TPN1 = math.random(-10,10)/500
	local TPN2 = math.random(-10,10)/500		
	Glitch1(Character,TPN1,.01,TPN2)
	WACKYEFFECT({
		Time = 45,
		EffectType = "Sphere",
		Size = VT(.5, 100000, .5),
		Size2 = VT(5, 100000, 5)*2,
		Transparency = 0,
		Transparency2 = 1,
		CFrame = CF(RootPart.Position),
		MoveToPos = nil,
		RotationX = 0,
		RotationY = 0,
		RotationZ = 0,
		Material = "Neon",
		Color = C3(1,0,0),
		SoundID = 1842442451,
		SoundPitch = 1.25,
		SoundVolume = 10,
		UseBoomerangMath = true,
		Boomerang = 200,
		SizeBoomerang = 190
	})
	WACKYEFFECT({
		Time = 45,
		EffectType = "Sphere",
		Size = VT(.25, 5000, .25),
		Size2 = VT(.5, 5000, .5),
		Transparency = 0,
		Transparency2 = 1,
		CFrame = CF(RootPart.Position),
		MoveToPos = nil,
		RotationX = MRANDOM(-360,360),
		RotationY = MRANDOM(-360,360),
		RotationZ = MRANDOM(-360,360),
		Material = "Neon",
		Color = C3(1,0,0),
		SoundID = nil,
		SoundPitch = nil,
		SoundVolume = nil,
		UseBoomerangMath = true,
		Boomerang = 200,
		SizeBoomerang = 190
	})
	WACKYEFFECT({
		Time = 45,
		EffectType = "Sphere",
		Size = VT(.25, 5000, .25),
		Size2 = VT(.5, 5000, .5),
		Transparency = 0,
		Transparency2 = 1,
		CFrame = CF(RootPart.Position),
		MoveToPos = nil,
		RotationX = MRANDOM(-360,360),
		RotationY = MRANDOM(-360,360),
		RotationZ = MRANDOM(-360,360),
		Material = "Neon",
		Color = C3(1,0,0),
		SoundID = nil,
		SoundPitch = nil,
		SoundVolume = nil,
		UseBoomerangMath = true,
		Boomerang = 200,
		SizeBoomerang = 190
	})
	WACKYEFFECT({
		Time = 45,
		EffectType = "Sphere",
		Size = VT(.25, 5000, .25),
		Size2 = VT(.5, 5000, .5),
		Transparency = 0,
		Transparency2 = 1,
		CFrame = CF(RootPart.Position),
		MoveToPos = nil,
		RotationX = MRANDOM(-360,360),
		RotationY = MRANDOM(-360,360),
		RotationZ = MRANDOM(-360,360),
		Material = "Neon",
		Color = C3(1,0,0),
		SoundID = nil,
		SoundPitch = nil,
		SoundVolume = nil,
		UseBoomerangMath = true,
		Boomerang = 200,
		SizeBoomerang = 190
	})
	POSITION:remove()
	SWORDS = false
	SOULSWORDS.Parent = nil
	NewHead.MainHead.RightEye.Fire1.Enabled = true
	NewHead.MainHead.RightEye.Fire2.Enabled = true
	NewHead.MainHead.RightEye.Fire3.Enabled = true
	NewHead.MainHead.LeftEye.Fire1.Enabled = true
	NewHead.MainHead.LeftEye.Fire2.Enabled = true
	NewHead.MainHead.LeftEye.Fire3.Enabled = true
	Head.Transparency = 0
	Head.CanCollide = true
	Torso.Transparency = 0
	Torso.CanCollide = true
	RightArm.Transparency = 0
	RightArm.CanCollide = true
	LeftArm.Transparency = 0
	LeftArm.CanCollide = true
	RightLeg.Transparency = 0
	LeftLeg.CanCollide = true
	LeftLeg.Transparency = 0
	for _, c in pairs(Character:GetChildren()) do
			if c and c.Parent and c.ClassName == "Accessory" then
				c.Handle.Transparency = 0              
			end
	end 
	BOOK.Parent = Character
	--[[FB1.Parent = Character
	FB2.Parent = Character
	FB3.Parent = Character
	FB4.Parent = Character
	FB5.Parent = Character
	FB6.Parent = Character]]
	DFIREBALL.Fire1.Enabled = true
	DFIREBALL.Fire2.Enabled = true
	DFIREBALL.Fire3.Enabled = true
	ATTACK = false
end

function BloodBullet()
	ATTACK = true
	local GYRO = IT("BodyGyro", RootPart)
	GYRO.D = 15
	GYRO.P = 2000
	GYRO.MaxTorque = VT(0, 4000000, 0)
	local FB1 = script.Bullet:Clone()
	FB1.Parent = Effects
	FB1.Transparency = 1
	local FB2 = script.Bullet:Clone()
	FB2.Parent = Effects
	FB2.Transparency = 1
	CreateSound("2959894855",FB1,3,2,false)
	CreateSound("902204346",FB2,5,1.5,false)
	coroutine.resume(coroutine.create(function()
		BOOK.Handle.Z.Color = Color3.new(1,0,0)
		BOOK.Handle.Z.Material = "Neon"
		Swait(15)
		BOOK.Handle.Z.Color = Color3.new(0,0,0)
		BOOK.Handle.Z.Material = "Ice"
		for i = 1,15 do
			Swait()
			FB1.Transparency = FB1.Transparency - 1/15
			FB1.Size = FB1.Size + Vector3.new(.25,0,0)
			FB2.Transparency = FB2.Transparency - 1/15
			FB2.Size = FB2.Size + Vector3.new(.25,0,0)
		end
	end))
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
		FB1.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(0,0,-10)*ANGLES(RAD(0),RAD(90),RAD(0))
		FB2.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(1,0,-10)*ANGLES(RAD(0),RAD(90),RAD(0))
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 3 + 2 * COS(SINE / 55)) * ANGLES(RAD(20), RAD(0), RAD(0)) * ANGLES(RAD(2 - 0.4 * COS(SINE / 32)), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(-5 + 4 * COS(SINE / 45)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
  		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.15, -0.15, -.75) * ANGLES(RAD(120 - 5 * COS(SINE / 38)), RAD(35 - 5 * COS(SINE / 35)), RAD(7.5 - 5 * COS(SINE / 32)))* RIGHTSHOULDERC0, 0.15 / 3)
        LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) *ANGLES(RAD(180),RAD(-30 + 10 * COS(SINE / 34)),RAD(-20 - 10 * COS(SINE / 34))) * LEFTSHOULDERC0, 0.15 / Animation_Speed)
  		RightHip.C0 = Clerp(RightHip.C0, CF(1, -.9 - .25 * COS(SINE / 22), -0.35) * ANGLES(RAD(-5 + 4 * COS(SINE / 34)), RAD(70 + 20 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)
        LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -.75 + .25 * COS(SINE / 22), -0.7) * ANGLES(RAD(-20 + 10 * COS(SINE / 32)), RAD(-80 - 10 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)		
	end
	for i=0, 2, 0.1 / Animation_Speed do
		Swait()
		GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
		FB1.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(0,0,-15)*ANGLES(RAD(0),RAD(90),RAD(0))
		FB2.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(1,0,-15)*ANGLES(RAD(0),RAD(90),RAD(0))
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 3 + 2 * COS(SINE / 55)) * ANGLES(RAD(20), RAD(0), RAD(0)) * ANGLES(RAD(2 - 0.4 * COS(SINE / 32)), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(-10 + 4 * COS(SINE / 45)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
  		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.15, -0.15, -.75) * ANGLES(RAD(120 - 5 * COS(SINE / 38)), RAD(35 - 5 * COS(SINE / 35)), RAD(7.5 - 5 * COS(SINE / 32)))* RIGHTSHOULDERC0, 0.15 / 3)
        LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) *ANGLES(RAD(90),RAD(-30 + 10 * COS(SINE / 34)),RAD(-20 - 10 * COS(SINE / 34))) * LEFTSHOULDERC0, 0.15 / Animation_Speed)
  		RightHip.C0 = Clerp(RightHip.C0, CF(1, -.9 - .25 * COS(SINE / 22), -0.35) * ANGLES(RAD(-5 + 4 * COS(SINE / 34)), RAD(70 + 20 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)
        LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -.75 + .25 * COS(SINE / 22), -0.7) * ANGLES(RAD(-20 + 10 * COS(SINE / 32)), RAD(-80 - 10 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)		
	end
	FB1.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(0,0,-15)*ANGLES(RAD(0),RAD(0),RAD(0))
	FB1.Transparency = 1
	--Bullet One
	coroutine.resume(coroutine.create(function()
		CreateSound("4988868452",RightArm,5,1.25,false)
		CameraShake(RootPart.Position,1e9,5,60)
		for i = 1, 250 do
			Swait()
			local HIT,POS,NORMAL = Raycast(FB1.Position,FB1.CFrame.lookVector,5,Character)
			if HIT then
				WACKYEFFECT({
					Time = 45,
					EffectType = "Sphere",
					Size = VT(1, 1, 1),
					Size2 = VT(8, 8, 8)*2,
					Transparency = 0,
					Transparency2 = 1,
					CFrame = CF(FB1.Position),
					MoveToPos = nil,
					RotationX = MRANDOM(-360,360),
					RotationY = MRANDOM(-360,360),
					RotationZ = MRANDOM(-360,360),
					Material = "Neon",
					Color = C3(1,0,0),
					SoundID = 174580476,
					SoundPitch = 1.25,
					SoundVolume = 5,
					UseBoomerangMath = false,
					Boomerang = 100,
					SizeBoomerang = 90
				})
				AOETime(FB1.Position,20)
				FB1:remove()
				break
			else
				AOETime(FB1.Position,15)
				FB1.CFrame = Clerp(FB1.CFrame,CF(FB1.Position,Mouse.Hit.p),0.001) * CF(0,0,-6)
				WACKYEFFECT({
							Time = 35,
							EffectType = "Bullet",
							Size = Vector3.new(1,1,1)/15,
							Size2 = Vector3.new(1,1,1)/15,
							Transparency = .55,
							Transparency2 = 1,
							CFrame = FB1.CFrame * CF(0, 0, 0) * CFrame.new(0,-.5,0) * ANGLES(RAD(0),RAD(90),RAD(0)),
							MoveToPos = nil,
							RotationX = 0,
							RotationY = 0,
							RotationZ = 0,
							Material = "Glass",
							Color = FB1.Color,
							SoundID = nil,
							SoundPitch = 2,
							SoundVolume = 10,
							UseBoomerangMath = true,
							Boomerang = 100,
							SizeBoomerang = 90
				})
				WACKYEFFECT({
							Time = 45,
							EffectType = "Bullet",
							Size = Vector3.new(1,1,1)/20,
							Size2 = Vector3.new(1,1,1)/20,
							Transparency = 0,
							Transparency2 = 1,
							CFrame = FB1.CFrame * CF(0, 0, 0) * CFrame.new(0,-.5,0) * ANGLES(RAD(0),RAD(90),RAD(0)),
							MoveToPos = nil,
							RotationX = 0,
							RotationY = 0,
							RotationZ = 0,
							Material = "Neon",
							Color = Color3.new(1,0,0),
							SoundID = nil,
							SoundPitch = 2,
							SoundVolume = 10,
							UseBoomerangMath = true,
							Boomerang = 100,
							SizeBoomerang = 90
				})
			end
		end
		FB1:remove()
	end))
	for i=0, 1.5, 0.1 / Animation_Speed do
		Swait()
		GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
		--FB1.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(0,0,-15)*ANGLES(RAD(0),RAD(90),RAD(0))
		FB2.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(1,0,-15)*ANGLES(RAD(0),RAD(90),RAD(0))
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 3 + 2 * COS(SINE / 55)) * ANGLES(RAD(20), RAD(0), RAD(0)) * ANGLES(RAD(2 - 0.4 * COS(SINE / 32)), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(-10 + 4 * COS(SINE / 45)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
  		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.15, -0.15, -.75) * ANGLES(RAD(120 - 5 * COS(SINE / 38)), RAD(35 - 5 * COS(SINE / 35)), RAD(7.5 - 5 * COS(SINE / 32)))* RIGHTSHOULDERC0, 0.15 / 3)
        LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) *ANGLES(RAD(90),RAD(-30 + 10 * COS(SINE / 34)),RAD(-90 - 10 * COS(SINE / 34))) * LEFTSHOULDERC0, 0.15 / Animation_Speed)
  		RightHip.C0 = Clerp(RightHip.C0, CF(1, -.9 - .25 * COS(SINE / 22), -0.35) * ANGLES(RAD(-5 + 4 * COS(SINE / 34)), RAD(70 + 20 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)
        LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -.75 + .25 * COS(SINE / 22), -0.7) * ANGLES(RAD(-20 + 10 * COS(SINE / 32)), RAD(-80 - 10 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)		
	end
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
		--FB1.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(0,0,-15)*ANGLES(RAD(0),RAD(90),RAD(0))
		FB2.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(1,0,-15)*ANGLES(RAD(0),RAD(90),RAD(0))
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 3 + 2 * COS(SINE / 55)) * ANGLES(RAD(20), RAD(0), RAD(0)) * ANGLES(RAD(2 - 0.4 * COS(SINE / 32)), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(-10 + 4 * COS(SINE / 45)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
  		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.15, -0.15, -.75) * ANGLES(RAD(120 - 5 * COS(SINE / 38)), RAD(35 - 5 * COS(SINE / 35)), RAD(7.5 - 5 * COS(SINE / 32)))* RIGHTSHOULDERC0, 0.15 / 3)
        LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) *ANGLES(RAD(90),RAD(-30 + 10 * COS(SINE / 34)),RAD(-20 - 10 * COS(SINE / 34))) * LEFTSHOULDERC0, 0.15 / Animation_Speed)
  		RightHip.C0 = Clerp(RightHip.C0, CF(1, -.9 - .25 * COS(SINE / 22), -0.35) * ANGLES(RAD(-5 + 4 * COS(SINE / 34)), RAD(70 + 20 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)
        LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -.75 + .25 * COS(SINE / 22), -0.7) * ANGLES(RAD(-20 + 10 * COS(SINE / 32)), RAD(-80 - 10 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)		
	end
	FB2.CFrame = CF(Torso.Position,Mouse.Hit.p) * CF(1,0,-15)*ANGLES(RAD(0),RAD(0),RAD(0))
	FB2.Transparency = 1
	--Bullet Two
	coroutine.resume(coroutine.create(function()
		CreateSound("4988868452",RightArm,5,1.25,false)
		CameraShake(RootPart.Position,1e9,5,60)
		for i = 1, 250 do
			Swait()
			local HIT,POS,NORMAL = Raycast(FB2.Position,FB2.CFrame.lookVector,5,Character)
			if HIT then
				WACKYEFFECT({
					Time = 45,
					EffectType = "Sphere",
					Size = VT(1, 1, 1),
					Size2 = VT(8, 8, 8)*2,
					Transparency = 0,
					Transparency2 = 1,
					CFrame = CF(FB2.Position),
					MoveToPos = nil,
					RotationX = MRANDOM(-360,360),
					RotationY = MRANDOM(-360,360),
					RotationZ = MRANDOM(-360,360),
					Material = "Neon",
					Color = C3(1,0,0),
					SoundID = 174580476,
					SoundPitch = 1.25,
					SoundVolume = 5,
					UseBoomerangMath = false,
					Boomerang = 100,
					SizeBoomerang = 90
				})
				AOETime(FB2.Position,20)
				FB2:remove()
				break
			else
				AOETime(FB1.Position,15)
				FB2.CFrame = Clerp(FB2.CFrame,CF(FB2.Position,Mouse.Hit.p),0.001) * CF(0,0,-6)
				WACKYEFFECT({
							Time = 35,
							EffectType = "Bullet",
							Size = Vector3.new(1,1,1)/15,
							Size2 = Vector3.new(1,1,1)/15,
							Transparency = .55,
							Transparency2 = 1,
							CFrame = FB2.CFrame * CF(0, 0, 0) * CFrame.new(0,-.5,0) * ANGLES(RAD(0),RAD(90),RAD(0)),
							MoveToPos = nil,
							RotationX = 0,
							RotationY = 0,
							RotationZ = 0,
							Material = "Glass",
							Color = FB2.Color,
							SoundID = nil,
							SoundPitch = 2,
							SoundVolume = 10,
							UseBoomerangMath = true,
							Boomerang = 100,
							SizeBoomerang = 90
				})
				WACKYEFFECT({
							Time = 45,
							EffectType = "Bullet",
							Size = Vector3.new(1,1,1)/20,
							Size2 = Vector3.new(1,1,1)/20,
							Transparency = 0,
							Transparency2 = 1,
							CFrame = FB2.CFrame * CF(0, 0, 0) * CFrame.new(0,-.5,0) * ANGLES(RAD(0),RAD(90),RAD(0)),
							MoveToPos = nil,
							RotationX = 0,
							RotationY = 0,
							RotationZ = 0,
							Material = "Neon",
							Color = Color3.new(1,0,0),
							SoundID = nil,
							SoundPitch = 2,
							SoundVolume = 10,
							UseBoomerangMath = true,
							Boomerang = 100,
							SizeBoomerang = 90
				})
			end
		end
		FB2:remove()
	end))
	GYRO:remove()
	ATTACK = false
end

function Fireball()
	ATTACK = true
	local GYRO = IT("BodyGyro", RootPart)
	GYRO.D = 15
	GYRO.P = 2000
	GYRO.MaxTorque = VT(0, 4000000, 0)
	coroutine.resume(coroutine.create(function()
		BOOK.Handle.F.Color = Color3.new(1,0,0)
		BOOK.Handle.F.Material = "Neon"
		Swait(15)
		BOOK.Handle.F.Color = Color3.new(0,0,0)
		BOOK.Handle.F.Material = "Ice"
	end))
	DFIREBALL.Fire1.Enabled = false
	DFIREBALL.Fire2.Enabled = false
	DFIREBALL.Fire3.Enabled = false
	local AFIREBALL = script.Fireball:Clone()
	AFIREBALL.Parent = Effects
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		AFIREBALL.CFrame = Torso.CFrame*CF(0,0,-10-(i/15)) * ANGLES(RAD(0), RAD(0), RAD(-2.5))
		GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 3 + 2 * COS(SINE / 55)) * ANGLES(RAD(0), RAD(0), RAD(0)) * ANGLES(RAD(2 - 0.4 * COS(SINE / 32)), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(-5 + 4 * COS(SINE / 45)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
  		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.15, -0.15, -.75) * ANGLES(RAD(120 - 5 * COS(SINE / 38)), RAD(35 - 5 * COS(SINE / 35)), RAD(7.5 - 5 * COS(SINE / 32)))* RIGHTSHOULDERC0, 0.15 / 3)
        LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) *ANGLES(RAD(90),RAD(-30 + 10 * COS(SINE / 34)),RAD(-20 - 10 * COS(SINE / 34))) * LEFTSHOULDERC0, 0.15 / Animation_Speed)
  		RightHip.C0 = Clerp(RightHip.C0, CF(1, -.9 - .25 * COS(SINE / 22), -0.35) * ANGLES(RAD(-5 + 4 * COS(SINE / 34)), RAD(70 + 20 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)
        LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -.75 + .25 * COS(SINE / 22), -0.7) * ANGLES(RAD(-20 + 10 * COS(SINE / 32)), RAD(-80 - 10 * COS(SINE / 42)), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.15 / Animation_Speed)		
	end
	for i=0, .5, 0.1 / Animation_Speed do
		Swait()
		AFIREBALL.CFrame = Torso.CFrame*CF(0,0,-10-(i/15)) * ANGLES(RAD(0), RAD(0), RAD(-2.5))
		GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 3 + 2 * COS(SINE / 55)) * ANGLES(RAD(0), RAD(0), RAD(0)) * ANGLES(RAD(2 - 0.4 * COS(SINE / 32)), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(-5 + 4 * COS(SINE / 45)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
  		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.15, -0.15, -.75) * ANGLES(RAD(120 - 5 * COS(SINE / 38)), RAD(35 - 5 * COS(SINE / 35)), RAD(7.5 - 5 * COS(SINE / 32)))* RIGHTSHOULDERC0, 0.15 / 3)
        LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) *ANGLES(RAD(90),RAD(-30 + 10 * COS(SINE / 34)),RAD(-90 - 10 * COS(SINE / 34))) * LEFTSHOULDERC0, 0.15 / Animation_Speed)
