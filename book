	for _, c in pairs(Character:GetChildren()) do
			if c and c.Parent and c.ClassName == "Accessory" then
				c.Handle.Transparency = 1
			end
	end
	WACKYEFFECT({Time = 12, EffectType = "Wave", Size = VT(1,.5,1), Size2 = VT(15,.25,15), Transparency = .95, Transparency2 = 1, CFrame = Torso.CFrame*CFrame.new(0,3.9,0)*CFrame.Angles(RAD(180),RAD(MRANDOM(-360, 360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = Color3.new(0,0,0), SoundID = nil, SoundPitch = nil, SoundVolume = nil, UserBoomerangMath = true, Boomerang = 100, SizeBoomerang = 90})
