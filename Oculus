function dekimmoment()
	ATTACK = true
	local a = 0
	for i = 0,75 do
		Swait()
		a = a + 1
		if a >= 5 then
			a = 0
		end
	local Alpha = .1
	local ct = {C3(1,1,1),C3(0,0,0)}
	local st = {"Sphere","Box"}
	WACKYEFFECT2({Time = 60, EffectType = st[MRANDOM(1,2)], Size = VT(10,10,10)/2, Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(MRANDOM(-15,15),MRANDOM(0,15),MRANDOM(-15,15)), MoveToPos = RootPart.Position, RotationX = MRANDOM(-8,8), RotationY = MRANDOM(-8,8), RotationZ = MRANDOM(-8,8), Material = "Neon", Color =ct[MRANDOM(1,2)],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
