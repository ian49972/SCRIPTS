end

function multiballs()
	ATTACK = true
	local ct = {C3(1,1,1),C3(0,0,0)}
	for i = 0,20 do
		Swait()
		  	local Alpha = .1
	lefthandw.C0 = lefthandw.C0:lerp(leftcf*CF(0,-5,0)*ANGLES(RAD(-20),0,0),Alpha)
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,3,0)*ANGLES(RAD(0),RAD(0),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-0.4,-1,0)*ANGLES(RAD(-4.5),RAD(0.3),RAD(-3.3)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.5,-0.6)*ANGLES(RAD(-5.3),RAD(-0.1),RAD(1.3)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.5,0.1)*ANGLES(RAD(-173.5),RAD(2.9),RAD(-18.1)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,0.6,0)*ANGLES(RAD(0),RAD(0),RAD(18.7)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(0,0,0.2)*ANGLES(RAD(-9.7),RAD(0),RAD(0))*NECKC0,Alpha)
	end
	local spheres = {}
	for i = 1,8 do
		local sph = CreatePart(3,Effects,"Neon",0,0,"White","daspheres",VT(1,1,1),true)
		sph.Color = ct[MRANDOM(1,2)]
		sph.CFrame = RootPart.CFrame*CF(MRANDOM(-10,10),14,0)*CF(0,MRANDOM(-4,4),0)
		table.insert(spheres,sph)
	end
	CreateSound(243711369,RootPart,5,1,false)
	local sp
	for v = 1,#spheres do
		sp = spheres[v]
		MakeForm(spheres[v],"Ball")
		WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(2,2,2), Transparency = 0, Transparency2 = 1, CFrame = spheres[v].CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = ct[MRANDOM(1,2)],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	end
	coroutine.resume(coroutine.create(function()
	for i = 1,#spheres do
	swait()
	spheres[i].CFrame = CF(spheres[i].Position,Mouse.Hit.p)
	end
	for i = 1,27 do
		Swait()
		for i = 1,#spheres do	
		spheres[i].CFrame = spheres[i].CFrame*CF(0,0,-3)
		end
		for i = 1,#spheres do
		local hit,pos = Raycast(spheres[i].Position,spheres[i].CFrame.lookVector,5,Character)
		if hit then break end
		end
	end
	for i = 1,#spheres do
	ApplyAoE(spheres[i].Position,20,0,0,0,true)
	end
	for i = 1,#spheres do
	spheres[i]:Destroy()	
	end
	for i = 1,#spheres do
	WACKYEFFECT2({Time = 50, EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(30,30,30), Transparency = 0, Transparency2 = 1, CFrame = spheres[i].CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(0,0,0),SoundID = 192410077, SoundPitch = 1, SoundVolume = 3, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	WACKYEFFECT2({Time = 50, EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(25,25,25), Transparency = 0, Transparency2 = 1, CFrame = spheres[i].CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = 3238758645, SoundPitch = 1, SoundVolume = 3, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	WACKYEFFECT2({Time = 60, EffectType = "Wave", Size = VT(5,.5,5), Size2 = VT(30,.5,30), Transparency = 0, Transparency2 = 1, CFrame = spheres[i].CFrame*ANGLES(0,0,RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-4,4), RotationZ = 0, Material = "Neon", Color = ct[MRANDOM(1,2)],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})

	end
	end))
	for i = 0,15 do
		Swait()
		  	local Alpha = .2
		lefthandw.C0 = lefthandw.C0:lerp(leftcf*CF(-2,-.7,4)*ANGLES(RAD(20),0,RAD(20)),Alpha)
  	RootJoint.C0 = RootJoint.C0:lerp(CF(-0.2,3,-0.2)*ANGLES(RAD(0),RAD(-22.8),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-0.4,-1,0)*ANGLES(RAD(-4.5),RAD(0.3),RAD(-3.3)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.5,-0.6)*ANGLES(RAD(-5.3),RAD(-0.1),RAD(1.3)),Alpha)
  	la.C0 = la.C0:lerp(CF(-0.4,0.3,-1)*ANGLES(RAD(29.1),RAD(4.5),RAD(58.3)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,0.6,0)*ANGLES(RAD(0),RAD(0),RAD(18.7)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(0.4,0,0.3)*ANGLES(RAD(-10.5),RAD(22.5),RAD(4.1))*NECKC0,Alpha)
	end
	ATTACK = false
end

function datp()
	ATTACK = true
	for i = 0,20 do
		Swait()
		  	local Alpha = .1
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,3,0.4)*ANGLES(RAD(24),RAD(0),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-0.4,-1.1,0.1)*ANGLES(RAD(-28.5),RAD(0.3),RAD(-3.3)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.2,-1)*ANGLES(RAD(-29.3),RAD(-0.1),RAD(1.3)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.4,0.1)*ANGLES(RAD(103.9),RAD(0),RAD(1.6)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.6,0.5,0.1)*ANGLES(RAD(103.9),RAD(0),RAD(-6.4)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(0,0,-0.1)*ANGLES(RAD(4.5),RAD(0),RAD(0))*NECKC0,Alpha)
	end
	local hf,hp = Raycast(Mouse.Hit.p+VT(0,1,0), (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 100, Character)
	if hf then
		
		hp = hp + VT(0,3.5,0)
		local po = RootPart.Position
	
		RootPart.CFrame = CF(hp,CF(po,hp)*CF(0,0,-100000).p)
		local a = RootPart.CFrame * CF(0,100,0)
	SpawnTrail(a.p,RootPart.Position-VT(0,2.9,0),true)
	SpawnTrail(a.p,RootPart.Position-VT(0,2.9,0),false)
	local ct = {C3(1,1,1),C3(0,0,0)}
	WACKYEFFECT2({Time = 40, EffectType = "Sphere", Size = VT(10,.1,10), Size2 = VT(60,.1,60), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-2.9,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(0,0,0),SoundID = 200633837, SoundPitch = 1, SoundVolume = 7, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	WACKYEFFECT2({Time = 40, EffectType = "Sphere", Size = VT(10,.1,10), Size2 = VT(50,.1,50), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-2.9,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = 233856115, SoundPitch = 1.3, SoundVolume = 7, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	WACKYEFFECT2({Time = 50, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(2,75,2), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-2.9,0)*ANGLES(RAD(90),0,0), MoveToPos = nil, RotationX = 0, RotationY = 4, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	WACKYEFFECT2({Time = 50, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(2,75,2), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-2.9,0)*ANGLES(RAD(90),0,RAD(90)), MoveToPos = nil, RotationX = 0, RotationY = 4, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(78,78,78)/1.5, Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-2.9,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(0,0,0),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 15})
	WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(68,68,68)/1.5, Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-2.9,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 15})
	for i = 1,7 do
	WACKYEFFECT2({Time = 80, EffectType = "Crystal", Size = VT(3,65,3), Size2 = VT(1,0,1), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-2.9,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-1,1), RotationZ = 0, Material = "Neon", Color = ct[MRANDOM(1,2)],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	end
	ApplyAoE(RootPart.Position,23,0,0,0,true)

		for i = 0,15 do
			Swait()
			 	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0.2,-0.3)*ANGLES(RAD(-31.1),RAD(0),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-0.4,-1.3,-0.1)*ANGLES(RAD(26.6),RAD(0.3),RAD(-3.3)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.8,-0.8)*ANGLES(RAD(25.8),RAD(-0.1),RAD(1.3)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.1,0.1)*ANGLES(RAD(-44.6),RAD(0),RAD(1.6)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.6,0.1,0.1)*ANGLES(RAD(-44.6),RAD(0),RAD(-6.4)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(0,0,-0.1)*ANGLES(RAD(4.5),RAD(0),RAD(0))*NECKC0,Alpha)
		end
	end
	ATTACK = false
end

function cs()
	if SONG == 1833183591 then
		SONG = 386849348
	elseif SONG == 386849348 then
		SONG = 216437282
	elseif SONG == 216437282 then
		SONG = 844659281
	elseif SONG == 844659281 then
		SONG = 3365121349
	elseif SONG == 3365121349 then
		SONG = 	1833183591
	end
end


--keys
function MouseDown(Mouse)
	if ATTACK == false then
	end
end

function MouseUp(Mouse)
HOLD = false
end

function KeyDown(Key)
	KEYHOLD = true
	if Key == "z" and ATTACK == false then
		threecolumns()
	end
	if Key == "c" and ATTACK == false then
		dalightningbal()
	end

	if Key == "x" and ATTACK == false then
		dekimmoment()
	end

	if Key == "v" and ATTACK == false then
		multiballs()
	end
	if Key == "b" and ATTACK == false then
		datp()
	end
	if Key == "m" then
		cs()
	end

	if Key == "b" and ATTACK == false then
		
	end

	if Key == "]" and ATTACK == false then
		if sick.Parent ~= Character then
			sick = IT("Sound",Torso)
			sick.SoundId = "rbxassetid://"..SONG
		end
	end
end

function KeyUp(Key)
	KEYHOLD = false
end

	Mouse.Button1Down:connect(function(NEWKEY)
		MouseDown(NEWKEY)
	end)
	Mouse.Button1Up:connect(function(NEWKEY)
		MouseUp(NEWKEY)
	end)
	Mouse.KeyDown:connect(function(NEWKEY)
		KeyDown(NEWKEY)
	end)
	Mouse.KeyUp:connect(function(NEWKEY)
		KeyUp(NEWKEY)
	end)

function unanchor()
	for _, c in pairs(Character:GetChildren()) do
		if c:IsA("BasePart") and c ~= RootPart then
			c.Anchored = false
		end
	end
	if UNANCHOR == true then
		RootPart.Anchored = false
	else
		RootPart.Anchored = true
	end
end

--loop stuff

Humanoid.Changed:connect(function(Jump)
	if Jump == "Jump" and (Disable_Jump == true) then
		Humanoid.Jump = false
	end
end)
local br = 0
while true do
	Swait()
	ANIMATE.Parent = nil
	for _,v in next, Humanoid:GetPlayingAnimationTracks() do
	    v:Stop();
	end
	SINE = SINE + CHANGE*2
	local TORSOVELOCITY = (RootPart.Velocity * VT(1, 0, 1)).magnitude
	local TORSOVERTICALVELOCITY = RootPart.Velocity.y
	local HITFLOOR,HITPOS,NORMAL = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 4+Humanoid.HipHeight, Character)
	local TiltVelocity = CF(RootPart.CFrame:vectorToObjectSpace(RootPart.Velocity/1.6))
	local WALKSPEEDVALUE = 12 / (Humanoid.WalkSpeed / 16)
	if TORSOVERTICALVELOCITY > 1 and HITFLOOR == nil then
		ANIM = "Jump"
		if ATTACK == false then
			RootJoint.C0 = Clerp(RootJoint.C0, ROOTC0 * CF(0, 0, 0 ) * ANGLES(RAD(-5), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0 , 0 + ((1) - 1)) * ANGLES(RAD(-25), RAD(0), RAD(0)), 1 / Animation_Speed)
			ra.C0 = Clerp(ra.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(-35), RAD(0), RAD(25 + 10 * COS(SINE / 12))), 1 / Animation_Speed)
			la.C0 = Clerp(la.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(-35), RAD(0), RAD(-25 - 10 * COS(SINE / 12))), 1 / Animation_Speed)
			rh.C0 = Clerp(rh.C0, CF(.5, -0.4, -0.6) * ANGLES(RAD(1), RAD(0), RAD(0)) * ANGLES(RAD(-1 * SIN(SINE / 6)), RAD(0), RAD(0)), 1 / Animation_Speed)
			lh.C0 = Clerp(lh.C0, CF(-.5, -1, 0) * ANGLES(RAD(0), RAD(5), RAD(0)) * ANGLES(RAD(-1 * SIN(SINE / 6)), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
	elseif TORSOVERTICALVELOCITY < -1 and HITFLOOR == nil then
		ANIM = "Fall"
		if ATTACK == false then
			RootJoint.C0 = Clerp(RootJoint.C0, ROOTC0 * CF(0, 0, 0 ) * ANGLES(RAD(15), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0 , 0 + ((1) - 1)) * ANGLES(RAD(15), RAD(0), RAD(0)), 1 / Animation_Speed)
			ra.C0 = Clerp(ra.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(35 - 4 * COS(SINE / 6)), RAD(0), RAD(45 + 10 * COS(SINE / 12))), 1 / Animation_Speed)
			la.C0 = Clerp(la.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(35 - 4 * COS(SINE / 6)), RAD(0), RAD(-45 - 10 * COS(SINE / 12))), 1 / Animation_Speed)
			rh.C0 = Clerp(rh.C0, CF(.5, -0.3, -0.7) * ANGLES(RAD(-25 + 5 * SIN(SINE / 12)), RAD(0), RAD(0)) * ANGLES(RAD(-1 * SIN(SINE / 6)), RAD(0), RAD(0)), 1 / Animation_Speed)
			lh.C0 = Clerp(lh.C0, CF(-.5, -0.8, -0.3) * ANGLES(RAD(-10), RAD(10), RAD(0)) * ANGLES(RAD(-1 * SIN(SINE / 6)), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
	elseif TORSOVELOCITY < 1 and HITFLOOR ~= nil then
		local Alpha = .1
		ANIM = "Idle"
		if ATTACK == false then
  		RootJoint.C0 = RootJoint.C0:lerp(CF(0,3+0.5*COS(SINE/32),0)*ANGLES(RAD(0),RAD(0),RAD(0))*ROOTC0,Alpha)
  		lh.C0 = lh.C0:lerp(CF(-0.5,-1,0)*ANGLES(RAD(-4.5 - 3.5*SIN(SINE/32)),RAD(0.3),RAD(-3.3+1*COS(SINE/32))),Alpha)
  		rh.C0 = rh.C0:lerp(CF(0.5,-0.5,-0.6)*ANGLES(RAD(-5.3 + 3.5*SIN(SINE/32)),RAD(-0.1),RAD(1.3-1*COS(SINE/32))),Alpha)
  		la.C0 = la.C0:lerp(CF(-1.5,0.6,0.1)*ANGLES(RAD(0+1.9*SIN(SINE/78)),RAD(0+2*COS(SINE/70)),RAD(-24.2- 6 * SIN(SINE/32))),Alpha)
  		ra.C0 = ra.C0:lerp(CF(1.5,0.6,0)*ANGLES(RAD(0+1.2*COS(SINE/60)),RAD(0+3*SIN(SINE/75)),RAD(18.7 + 6 * SIN(SINE/32))),Alpha)
  		Neck.C0 = Neck.C0:lerp(NECKC0*CF(0,0,0+ ((1) - 1))*ANGLES(RAD(9.7-5*SIN(SINE/32)),RAD(0),RAD(0+8*SIN(SINE/74))),Alpha)
		end
	elseif TORSOVELOCITY > 1 and HITFLOOR ~= nil then
		ANIM = "Walk"
		if ATTACK == false then
		local Alpha = .1
  		RootJoint.C0 = RootJoint.C0:lerp(CF(0,3+0.5*COS(SINE/25),0.1)*ANGLES(RAD(-5.3+3*SIN(SINE/25)+RAD(TiltVelocity.Z*45)),RAD(0),RAD(0-RAD(TiltVelocity.X*10)))*ROOTC0,Alpha)
  		lh.C0 = lh.C0:lerp(CF(-0.5,-1,0)*ANGLES(RAD(-16.2+5*SIN(SINE/25)+RAD(TiltVelocity.Z*5)),RAD(0),RAD(0-RAD(TiltVelocity.X*19))),Alpha)
  		rh.C0 = rh.C0:lerp(CF(0.5,-0.3,-0.6)*ANGLES(RAD(-20.4+5*SIN(SINE/25)+RAD(TiltVelocity.Z*5)),RAD(0),RAD(0-RAD(TiltVelocity.X*19))),Alpha)
  		la.C0 = la.C0:lerp(CF(-1.4,0.5+0.05*COS(SINE/25),0)*ANGLES(RAD(0.6),RAD(3.8),RAD(-9+3.6*SIN(SINE/25))),Alpha)
  		ra.C0 = ra.C0:lerp(CF(1.4,0.5+0.05*COS(SINE/25),0)*ANGLES(RAD(0.9),RAD(-8.1),RAD(6.5-3.6*SIN(SINE/25))),Alpha)
  		Neck.C0 = Neck.C0:lerp(NECKC0*CF(0,0,0+ ((1) - 1))*CF(0,0,0)*ANGLES(RAD(-5.3+3*SIN(SINE/25)+RAD(TiltVelocity.Z*45)),RAD(0),RAD(-2*COS(SINE/25))-RAD(TiltVelocity.X*1.5)),Alpha)
		end
	end
	if ATTACK == false then
		mw.C0 = mw.C0:lerp(CF(0,-3 - .3 *SIN(SINE/35),-1),.1)
		righthw.C0 = righthw.C0:lerp(CF(-8,.5,-.5)*ANGLES(RAD(0+5*COS(SINE/65)),RAD(0+3*COS(SINE/75)),RAD(0+2.7*SIN(SINE/69))),.1)
		lefthandw.C0 = lefthandw.C0:lerp(CF(8,.5,-.5)*ANGLES(RAD(0+2.7*SIN(SINE/69)),RAD(0+5*COS(SINE/65)),RAD(0+3*COS(SINE/75))),.1)
		--

		--
		for q = 1,#lfingermparts do
			lfingermparts[q].C0 = lfingermparts[q].C0:lerp(d*ANGLES(RAD(0+5*COS(SINE/45)),0,0),.1)
		end
				lefthand.thumb.finger1.C0 = lefthand.thumb.finger1.C0:lerp(d*ANGLES(RAD(0-5*COS(SINE/45)),0,0),.1)
		for e = 1,#lfingerparts2 do
			lfingerparts2[e].C0 = lfingerparts2[e].C0:lerp(d*ANGLES(RAD(0),RAD(-10+16*COS(SINE/45)),0),.1)
		end
		lefthand.thumb.Cylinder.finger2.C0 = lefthand.thumb.Cylinder.finger2.C0:lerp(d*ANGLES(RAD(0),RAD(10-6*COS(SINE/45)),0),.1)
		for r = 1,#lfingerparts3 do
			lfingerparts3[r].C0 = lfingerparts3[r].C0:lerp(d*ANGLES(RAD(-10+8*COS(SINE/45)),0,0),.1)
		end
			for t= 1,#rfingermparts do
			rfingermparts[t].C0 = rfingermparts[t].C0:lerp(d*ANGLES(RAD(0+5*COS(SINE/45)),0,0),.1)
		end
				righthand.thumb.finger1.C0 = righthand.thumb.finger1.C0:lerp(d*ANGLES(RAD(0-5*COS(SINE/45)),0,0),.1)
		for y = 1,#rfingerparts2 do
			rfingerparts2[y].C0 = rfingerparts2[y].C0:lerp(d*ANGLES(RAD(0),RAD(-10+16*COS(SINE/45)),0),.1)
		end
		righthand.thumb.Cylinder.finger2.C0 = righthand.thumb.Cylinder.finger2.C0:lerp(d*ANGLES(RAD(0),RAD(10-6*COS(SINE/45)),0),.1)
		for u = 1,#rfingerparts3 do
			rfingerparts3[u].C0 = rfingerparts3[u].C0:lerp(d*ANGLES(RAD(-10+8*COS(SINE/45)),0,0),.1)
		end
	end
			leftmcog.C0 = leftmcog.C0:lerp(CF(0,0,0)*ANGLES(0,0,RAD(SINE)),.1)
		rightmcog.C0 = rightmcog.C0:lerp(CF(0,0,0)*ANGLES(0,0,RAD(-SINE)),.1)
		lhcog.C0 = lhcog.C0:lerp(d*ANGLES(0,0,RAD(SINE)),.1)
		rhcog.C0 = rhcog.C0:lerp(d*ANGLES(0,0,RAD(-SINE)),.1)
	local col = Color3.fromRGB(127.5+127.5*COS(SINE/50),127.5+127.5*COS(SINE/50),127.5+127.5*COS(SINE/50))
	br = br + 1
	if br >= 5 then
		br = 0
	end
	if br == 1 then
		local t = {-6,6}
	WACKYEFFECT2({Time = 40, EffectType = "Wave", Size = VT(1,1,1), Size2 = VT(12-3*COS(SINE/50),.5,12-3*COS(SINE/50)), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-2.9,0), MoveToPos = nil, RotationX = 0, RotationY = t[MRANDOM(1,2)]+MRANDOM(-2,2), RotationZ = 0, Material = "Neon", Color = col,SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	end
	unanchor()
	Humanoid.MaxHealth = 1e4
	Humanoid.Health = 1e4
	if Rooted == false then
		Disable_Jump = false
		Humanoid.WalkSpeed = Speed
	elseif Rooted == true then
		Disable_Jump = true
		Humanoid.WalkSpeed = 0
	end
	sick.SoundId = "rbxassetid://"..SONG
	sick.Looped = true
	sick.Pitch = 1
	sick.Volume = 3
	sick.Playing = true
end
