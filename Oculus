  	la.C0 = la.C0:lerp(CF(-1.5,0.5,0.3)*ANGLES(RAD(-35.9),RAD(0),RAD(-24.2)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,0.4,0.3)*ANGLES(RAD(-35.9),RAD(0),RAD(18.7)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(0,0.1,-0.7)*ANGLES(RAD(25.7),RAD(0),RAD(0))*NECKC0,Alpha)
local ct = {C3(1,1,1),C3(0,0,0)}
	WACKYEFFECT2({Time = 60, EffectType = "Box", Size = VT(1+i/5,1+i/5,1+i/5), Size2 = VT(250+i*2,250+i*2,250+i*2), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame, MoveToPos = nil, RotationX = MRANDOM(-15,15), RotationY = MRANDOM(-15,15), RotationZ = MRANDOM(-15,15), Material = "Neon", Color = ct[MRANDOM(1,2)],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
