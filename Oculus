	end
	CreateSound(192410084,Character,5,.5,false)
	for i = 1,200 do
		Swait()
		  local Alpha = .1
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0,0.5)*ANGLES(RAD(34.3),RAD(0),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-0.4,-1.1,0.2)*ANGLES(RAD(-38.9),RAD(0.3),RAD(-3.3)),Alpha)
