for i,v in pairs(char:GetChildren()) do
	if (v:IsA("BasePart") and NewChar:FindFirstChild(v.Name)) then
		ins("Motor6D",{Parent=NewChar;Part0=v;Part1=NewChar[v.Name]})
	end
end
