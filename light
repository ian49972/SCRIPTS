		--end
		proj:Destroy()
		end))	
		end
	until don == true
	SetTween(portl,{Color = cols[2],Size = VT(0,0,0)},"Sine","InOut",.5)
	Debris:AddItem(portl,.51)
	end))
	ATTACK = false
end

function tclms()
	ATTACK = true
	for i = 0,20 do
		Swait()
		  	local Alpha = .1
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0,0)*ANGLES(RAD(0),RAD(0),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-1.1,-0.8,-0.9)*ANGLES(RAD(-4.9),RAD(2.3),RAD(102.2)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.7,-0.5)*ANGLES(RAD(0.2),RAD(-3.8),RAD(1.9)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.1,0.6,-0.4)*ANGLES(RAD(170.2),RAD(18.5),RAD(14.9)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,0.5,-0.1)*ANGLES(RAD(-8.5),RAD(15.2),RAD(14.5)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(0,0,0.2)*ANGLES(RAD(-8.5),RAD(0),RAD(0))*NECKC0,Alpha)
	end
	for i = 0,20 do
		Swait()
		  	local Alpha = .2
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0,0)*ANGLES(RAD(0),RAD(0),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-1.1,-0.8,-0.9)*ANGLES(RAD(-4.9),RAD(2.3),RAD(102.2)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.7,-0.5)*ANGLES(RAD(0.2),RAD(-3.8),RAD(1.9)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.4,0.5,-0.1)*ANGLES(RAD(-9.5),RAD(-18.6),RAD(-92.9)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,0.5,-0.1)*ANGLES(RAD(-8.5),RAD(15.2),RAD(14.5)),Alpha)
