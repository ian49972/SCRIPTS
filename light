	end
	local rr = RootPart.CFrame
	local hf,hp = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 3.7+Humanoid.HipHeight, Character)
	if hf then
	coroutine.resume(coroutine.create(function()
	for i = 1,9 do
	swait(.15)
	local p1,p2,p3 = rr*CF(0,0,-i*25),rr*CF(-30,0,-i*25)*ANGLES(0,RAD(45),0),rr*CF(30,0,-i*25)*ANGLES(0,RAD(-45),0)
	local pt = {p1,p2,p3}
	for _,g in pairs(pt) do
	local hf2,hp2 = Raycast(g.p+VT(0,1,0), (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 3.7+Humanoid.HipHeight, Character)
	local ab = g*CF(0,100,0)
	SpawnTrail(ab.p,hp2,"one",10,45,1)
	SpawnTrail(ab.p,hp2,"two",5,35,1)
	ApplyAoE(hp2,30,0,0,0,true)
	WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,.1,1), Size2 = VT(60,.1,60), Transparency = 0, Transparency2 = 1, CFrame = CF(hp2), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
	WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,.1,1), Size2 = VT(50,.1,50), Transparency = 0, Transparency2 = 1, CFrame = CF(hp2), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[2],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
	WACKYEFFECT2({Time = 60, EffectType = "Wave", Size = VT(1,1,1), Size2 = VT(65,1,65), Transparency = 0, Transparency2 = 1, CFrame = CF(hp2), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-8,8), RotationZ = 0, Material = "Neon", Color = cols[2],SoundID = 192410077, SoundPitch = 1, SoundVolume = 5, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
	end	
	end
	end))
	end
	ATTACK = false
end

local activezeds = {}
local zeds = false

function dnvoke()
	ATTACK = true
	local msp
	local circl = CreatePart(3,Effects,"Neon",0,0,"Grey","cir",VT(1,.1,1),true)
	MakeForm(circl,"Ball")
	circl.Color = cols[2]
	circl.CFrame = CF(RootPart.Position-VT(0,Humanoid.HipHeight,0))*CF(0,-2.9,0)
	SetTween(circl,{Size = VT(108,.1,108),Color = cols[1]},"Sine","InOut",2)
	CreateSound(233856115,circl,5,.5,false)
	for i = 0,120  do
		Swait()
	local Alpha = .1
	if MRANDOM(1,2) == 1 then
	WACKYEFFECT2({Time = 45, EffectType = "Sphere", Size = VT(3,4,3)*2, Size2 = VT(1,7,1)*2, Transparency = 0, Transparency2 = 1, CFrame = circl.CFrame*CF(MRANDOM(-circl.Size.X,circl.Size.X)/2,0,MRANDOM(-circl.Size.X,circl.Size.X)/2)*ANGLES(0,0,RAD(MRANDOM(-15,15))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon",Color = circl.Color,SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})	
	end
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0,0)*ANGLES(RAD(0),RAD(0),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-1.1,-0.8,-0.9)*ANGLES(RAD(-4.9),RAD(2.3),RAD(102.2)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.7,-0.5)*ANGLES(RAD(0.2),RAD(-3.8),RAD(1.9)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.4,-0.1)*ANGLES(RAD(-9.5),RAD(-18.6),RAD(-99.4)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,0.4,-0.1)*ANGLES(RAD(-8.5),RAD(15.2),RAD(100.1)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(0,0.2,-0.6)*ANGLES(RAD(26),RAD(0),RAD(0))*NECKC0,Alpha)
	end
	for _,b in pairs(FindNearestHead(circl.Position,60)) do
	if b:FindFirstChild("Head") and b.Name ~= "lightzomb" then
	local accs = {}
	local minion = zmb:Clone()
	minion.Parent = workspace
	table.insert(activezeds,minion)
	zeds = true
	if not b:FindFirstChild("HumanoidRootPart") then
	msp = b.Torso
	else
	msp = b.HumanoidRootPart	
	end
	minion.AI.Disabled = false
	minion:SetPrimaryPartCFrame(msp.CFrame)
	minion.Creator.Value = Character
	minion.Humanoid.Died:Connect(function()
	if minion:FindFirstChild("Torso") then
	ApplyAoE(minion.Torso.Position,10,0,0,0,true)
	end
	end)
	for _,c in pairs(b:GetChildren()) do
		if c:IsA("Accessory") then
			--pcall(function()
			c.Parent = minion
			c.Handle:FindFirstChildOfClass("SpecialMesh").TextureId = "http://www.roblox.com/asset/?id="
			c.Handle:FindFirstChildOfClass("SpecialMesh").VertexColor = Vector3.new(200, 200, 200)
			minion.Humanoid:AddAccessory(c)
			--end)
		end
	end
	lightup2(b)
	end	
	end
	delay(.75,function()
	SetTween(circl,{Size = VT(0,.1,0),Color = cols[2]},"Sine","InOut",2)
	Debris:AddItem(circl,2.1)
	end)
	ATTACK = false
end

function explodezeds()
	for _,h in pairs(activezeds) do
		if h:FindFirstChild("Torso") then
		WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(20,20,20), Transparency = 0, Transparency2 = 1, CFrame = CF(h.Torso.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = 	231917970, SoundPitch = 1.75, SoundVolume = 5, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
		WACKYEFFECT2({Time = 60, EffectType = "Box", Size = VT(1,1,1), Size2 = VT(18,18,18), Transparency = 0, Transparency2 = 1, CFrame = CF(h.Torso.Position), MoveToPos = nil, RotationX = MRANDOM(-12,12), RotationY = MRANDOM(-12,12), RotationZ = MRANDOM(-12,12), Material = "Neon", Color = cols[1],SoundID = 	nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
		ApplyAoE(h.Torso.Position,10,0,0,0,true)
		h:Destroy()
		end
	end
	activezeds = {}
end

function twpil()
	ATTACK = true
	local swrdd = CreatePart(3,Effects,"Neon",0,1,"White","pilotredsun is cool",VT(0.2,0.5,4.5),false)
	local orig = swrdd.CFrame
	SetTween(swrdd,{Transparency = 0},"Sine","InOut",.5)
	swrdd.Massless = true
	local m = CreateMesh("SpecialMesh",swrdd,"FileMesh","409664704","",VT(2.5,2.5,2.5),VT(0,0,0))
	swrdd.Color = cols[1]
	local w = weld(swrdd,swrdd,RightArm,CF(0,-1,1.75)*ANGLES(RAD(-180),0,0),d)
	for i = 0,40 do
		Swait()
		  	local Alpha = .1
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0.1,0,0)*ANGLES(RAD(0),RAD(-58.7),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-1.1,-0.8,-0.9)*ANGLES(RAD(-4.9),RAD(2.3),RAD(102.2)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.7,-0.5)*ANGLES(RAD(0.2),RAD(-3.8),RAD(1.9)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.5,-0.1)*ANGLES(RAD(-9.5),RAD(-18.6),RAD(-14.8)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,1,-0.6)*ANGLES(RAD(147.9),RAD(-10.2),RAD(-19.4)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(0.1,0,0.2)*ANGLES(RAD(-16),RAD(57.7),RAD(13.7))*NECKC0,Alpha)
	end

	local hit,pos = CastProperRay(orig.p,  Mouse.Hit.p, 500, Character)
	local dist = (pos-orig.p).Magnitude
	SpawnTrail(swrdd.Position,pos,"one",3,8,.55)	
	swrdd:Destroy() --
	CreateSound(168586621,RightArm,7,1,false)
	WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(25,25,25)*2, Transparency = 0, Transparency2 = 1, CFrame = CF(pos), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = 231917970, SoundPitch = 1, SoundVolume = 10, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
	for i = 1,6 do
	WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1.3,2.3,1.3), Size2 = VT(1,35,1)*2.65, Transparency = 0, Transparency2 = 1, CFrame = CF(pos)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
	end
	ApplyAoE(pos,30,0,0,0,true)
	WACKYEFFECT2({Time = 60, EffectType = "Box", Size = VT(1,1,1), Size2 = VT(22,22,22)*1.75, Transparency = 0, Transparency2 = 1, CFrame = CF(pos), MoveToPos = nil, RotationX = MRANDOM(-10,10), RotationY = MRANDOM(-10,10), RotationZ = MRANDOM(-10,10), Material = "Neon", Color = cols[1],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
	local point1,point2,point3,point4 = CF(pos),CF(pos),CF(pos),CF(pos)
	local poo = 0
	coroutine.resume(coroutine.create(function()
	for i = 1,35 do
		poo = poo + 1 if poo >=4 then poo = 0 end
		Swait()
		point1,point2,point3,point4 = point1*CF(i/6,0,0),point2*CF(-i/6,0,0),point3*CF(0,0,i/6),point4*CF(0,0,-i/6)
		WACKYEFFECT2({Time = 30, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(20-i/2,205-i*3,3-i/2), Transparency = 0, Transparency2 = 1, CFrame = point1, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
		WACKYEFFECT2({Time = 30, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(20-i/2,205-i*3,3-i/2), Transparency = 0, Transparency2 = 1, CFrame = point2, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
		WACKYEFFECT2({Time = 30, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(20-i/2,205-i*3,3-i/2), Transparency = 0, Transparency2 = 1, CFrame = point3, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
		WACKYEFFECT2({Time = 30, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(20-i/2,205-i*3,3-i/2), Transparency = 0, Transparency2 = 1, CFrame = point4, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
		if poo == 1 then ApplyAoE(point1.p,20,0,0,0,true) ApplyAoE(point2.p,20,0,0,0,true) ApplyAoE(point3.p,20,0,0,0,true) ApplyAoE(point4.p,20,0,0,0,true) end
	end	
	end))
	local pil = CF(pos)*CF(0,100,0)
	SpawnTrail(pil.p,pos,"one",25,50,1)
	SpawnTrail(pil.p,pos,"one",15,40,1)
	for i = 0,10 do
		Swait()
		 	local Alpha = .25
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0,-0.1)*ANGLES(RAD(0),RAD(45.6),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-1.1,-0.8,-0.9)*ANGLES(RAD(-4.9),RAD(2.3),RAD(102.2)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.7,-0.5)*ANGLES(RAD(0.2),RAD(-3.8),RAD(1.9)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.5,-0.1)*ANGLES(RAD(-9.5),RAD(-18.6),RAD(-14.8)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(-0.1,0.4,-1)*ANGLES(RAD(98.9),RAD(21.3),RAD(-95.8)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(-0.2,0,0.2)*ANGLES(RAD(-12.1),RAD(-44.9),RAD(-8.6))*NECKC0,Alpha)
	end
	ATTACK = false
end

function trgmv()
	ATTACK = true
	if Mouse.Target.Parent ~= Character and Mouse.Target.Parent.Parent ~= Character and Mouse.Target.Parent:FindFirstChildOfClass("Humanoid") ~= nil then
	for i = 0,20 do
		Swait()
		  	local Alpha = .1
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0.1,0,0)*ANGLES(RAD(3),RAD(16.4),RAD(-0.8))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-1.1,-0.8,-0.9)*ANGLES(RAD(-4.9),RAD(2.3),RAD(102.2)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.7,-0.5)*ANGLES(RAD(0.2),RAD(-3.8),RAD(1.9)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.5,-0.1)*ANGLES(RAD(-9.5),RAD(-18.6),RAD(-14.8)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.5,0.5,0.2)*ANGLES(RAD(151.7),RAD(-9.3),RAD(16.7)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(0,0,-0.3)*ANGLES(RAD(10.8),RAD(-15.9),RAD(3.8))*NECKC0,Alpha)
	end
	pcall(function()
	local hum = Mouse.Target.Parent:FindFirstChild("Humanoid")
	local ttors = Mouse.Target.Parent:FindFirstChild("Torso") or Mouse.Target.Parent:FindFirstChild("UpperTorso")
	if ttors and hum.Health > 0 then
	ttors.Anchored = true
	SetTween(ttors,{CFrame = ttors.CFrame*CF(0,18,0)},"Back","InOut",1)
	local sphs = {}
	delay(.75,function()
	CreateSound(461105534,ttors,5,1.2,false)
	for i = 1,24 do
	local brbr = CreatePart(3,Effects,"Neon",0,1,"White","brgrgg",VT(1,1,1)*2,true)
	table.insert(sphs,brbr)
	MakeForm(brbr,"Ball")
	brbr.CFrame = ttors.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)))
	SetTween(brbr,{Transparency = 0,CFrame = brbr.CFrame*CF(0,0,-14)},"Sine","InOut",.7)
	end
	coroutine.resume(coroutine.create(function()
	swait(1.5)
	for _,p in pairs(sphs) do
	Swait(3)
	SpawnTrail(p.Position,ttors.Position,"one",2,2.2,.8)
	WACKYEFFECT2({Time = 30, EffectType = "Sphere", Size = p.Size, Size2 = p.Size*2, Transparency = 0, Transparency2 = 1, CFrame = p.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = 191395766, SoundPitch = 1, SoundVolume = 4, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	for i = 1,2 do
	WACKYEFFECT2({Time = 20, EffectType = "Sphere", Size = VT(.7,.7,.7)*2, Size2 = VT(.7,15,.7)*2, Transparency = 0, Transparency2 = 1, CFrame = ttors.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = cols[1],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
	end
	ApplyDamage(hum,math.ceil(hum.Health/10))
	p:Destroy()
	end	
	if hum.Parent then
	lightup2(hum.Parent)	
	end
	end))	
	end)	
	end
	end)
	end
	ATTACK = false
end

function tp()
	ATTACK = true
		local hf,hp = Raycast(Mouse.Hit.p+VT(0,1,0), (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 100, Character)
	if hf then
		hp = hp + VT(0,3.5+Humanoid.HipHeight,0)
		local po = RootPart.Position
		RootPart.CFrame = CF(hp,CF(po,hp)*CF(0,0,-100000).p)
		local a = RootPart.CFrame * CF(0,100,0)
		SpawnTrail(a.p,a.p-VT(0,200,0),"one",20,30,1)
		SpawnTrail(a.p,a.p-VT(0,200,0),"two",10,20,1)
		WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(20,20,20)*2, Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = 233091205, SoundPitch = 1.5, SoundVolume = 6, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
		for i = 1,6 do
		WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(0,60,0), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
		end
	end
	ATTACK = false
end

function cs()
	if SONG == 4101611542 then
		SONG = 4729746080
	elseif SONG == 4729746080 then
	SONG = 4728951859
	elseif SONG == 4728951859 then
	SONG = 4725462803
	elseif SONG == 4725462803 then
	SONG = 4725021231
	elseif SONG == 4725021231 then
	SONG = 214991711	
	elseif SONG == 214991711 then
	SONG = 4101611542				
	end
end


--keys
function MouseDown(Mouse)
	if ATTACK == false then
	end
end

function MouseUp(Mouse)
HOLD = false
end

function KeyDown(Key)
	KEYHOLD = true
	if Key == "z" and ATTACK == false then
		rlrda()
	end
	if Key == "x" and ATTACK == false then
		tclms()
	end

	if Key == "c" and ATTACK == false then
		dnvoke()
	end

	if Key == "v" and ATTACK == false then
		twpil()
	end

	if Key == "b" and ATTACK == false then
		trgmv()
	end
	
	if Key == "m" then cs() end
	
	if Key == "e" and ATTACK == false then tp() end
	
	if Key == "q" and zeds == true then explodezeds() zeds = false end
	
	if Key == "t" and ATTACK == false then 	
	CreateSound(4721101351,RootPart,10,1,false)
	chat(.75,.75,6,"Nature demands equilibrium, balance is perfection.")
	end

	if Key == "]" and ATTACK == false then
		if sick.Parent ~= Character then
			sick = IT("Sound",Torso)
			sick.SoundId = "rbxassetid://"..SONG
		end
	end
end

function KeyUp(Key)
	KEYHOLD = false
end

	Mouse.Button1Down:connect(function(NEWKEY)
		MouseDown(NEWKEY)
	end)
	Mouse.Button1Up:connect(function(NEWKEY)
		MouseUp(NEWKEY)
	end)
	Mouse.KeyDown:connect(function(NEWKEY)
		KeyDown(NEWKEY)
	end)
	Mouse.KeyUp:connect(function(NEWKEY)
		KeyUp(NEWKEY)
	end)

function unanchor()
	for _, c in pairs(Character:GetChildren()) do
		if c:IsA("BasePart") and c ~= RootPart then
			c.Anchored = false
		end
	end
	if UNANCHOR == true then
		RootPart.Anchored = false
	else
		RootPart.Anchored = true
	end
end

--loop stuff

Humanoid.Changed:connect(function(Jump)
	if Jump == "Jump" and (Disable_Jump == true) then
		Humanoid.Jump = false
	end
end)

loop = 0
loopval = 0

while true do
	Swait()
	ANIMATE.Parent = nil
	for _,v in next, Humanoid:GetPlayingAnimationTracks() do
	    v:Stop();
	end
	loop = loop + 1 if loop >= loopval then loop = 0 end
	SINE = SINE + CHANGE*2
	local TORSOVELOCITY = (RootPart.Velocity * VT(1, 0, 1)).magnitude
	local TORSOVERTICALVELOCITY = RootPart.Velocity.y
	local HITFLOOR,HITPOS,NORMAL = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 4+Humanoid.HipHeight, Character)
	local TiltVelocity = CF(RootPart.CFrame:vectorToObjectSpace(RootPart.Velocity/1.6))
	local WALKSPEEDVALUE = 12 / (Humanoid.WalkSpeed / 16)
	if TORSOVERTICALVELOCITY > 1 and HITFLOOR == nil then
		ANIM = "Jump"
		if ATTACK == false then
			RootJoint.C0 = Clerp(RootJoint.C0, ROOTC0 * CF(0, 0, 0 ) * ANGLES(RAD(-5), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0 , 0 + ((1) - 1)) * ANGLES(RAD(-25), RAD(0), RAD(0)), 1 / Animation_Speed)
			ra.C0 = Clerp(ra.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(-35), RAD(0), RAD(25 + 10 * COS(SINE / 12))), 1 / Animation_Speed)
			la.C0 = Clerp(la.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(-35), RAD(0), RAD(-25 - 10 * COS(SINE / 12))), 1 / Animation_Speed)
			rh.C0 = Clerp(rh.C0, CF(.5, -0.4, -0.6) * ANGLES(RAD(1), RAD(0), RAD(0)) * ANGLES(RAD(-1 * SIN(SINE / 6)), RAD(0), RAD(0)), 1 / Animation_Speed)
			lh.C0 = Clerp(lh.C0, CF(-.5, -1, 0) * ANGLES(RAD(0), RAD(5), RAD(0)) * ANGLES(RAD(-1 * SIN(SINE / 6)), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
	elseif TORSOVERTICALVELOCITY < -1 and HITFLOOR == nil then
		ANIM = "Fall"
		if ATTACK == false then
			RootJoint.C0 = Clerp(RootJoint.C0, ROOTC0 * CF(0, 0, 0 ) * ANGLES(RAD(15), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0 , 0 + ((1) - 1)) * ANGLES(RAD(15), RAD(0), RAD(0)), 1 / Animation_Speed)
			ra.C0 = Clerp(ra.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(35 - 4 * COS(SINE / 6)), RAD(0), RAD(45 + 10 * COS(SINE / 12))), 1 / Animation_Speed)
			la.C0 = Clerp(la.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(35 - 4 * COS(SINE / 6)), RAD(0), RAD(-45 - 10 * COS(SINE / 12))), 1 / Animation_Speed)
			rh.C0 = Clerp(rh.C0, CF(.5, -0.3, -0.7) * ANGLES(RAD(-25 + 5 * SIN(SINE / 12)), RAD(0), RAD(0)) * ANGLES(RAD(-1 * SIN(SINE / 6)), RAD(0), RAD(0)), 1 / Animation_Speed)
			lh.C0 = Clerp(lh.C0, CF(-.5, -0.8, -0.3) * ANGLES(RAD(-10), RAD(10), RAD(0)) * ANGLES(RAD(-1 * SIN(SINE / 6)), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
	elseif TORSOVELOCITY < 1 and HITFLOOR ~= nil then
		local Alpha = .1
		ANIM = "Idle"
		if ATTACK == false then
  		RootJoint.C0 = RootJoint.C0:lerp(CF(0,0+.75*COS(SINE/35),0)*ANGLES(RAD(0),RAD(0),RAD(0))*ROOTC0,Alpha)
  		lh.C0 = lh.C0:lerp(CF(-1.1,-0.8-.06*SIN(SINE/35),-0.9)*ANGLES(RAD(-4.9),RAD(2.3),RAD(102.2)),Alpha)
  		rh.C0 = rh.C0:lerp(CF(0.5,-0.7-.06*SIN(SINE/35),-0.5)*ANGLES(RAD(0.2+2*COS(SINE/35)),RAD(-3.8),RAD(1.9+1.65*SIN(SINE/35))),Alpha)
  		la.C0 = la.C0:lerp(CF(-1.5,0.5+.08*SIN(SINE/35),-0.1)*ANGLES(RAD(-9.5),RAD(-18.6-2*COS(SINE/35)),RAD(-14.8-6*COS(SINE/35))),Alpha)
  		ra.C0 = ra.C0:lerp(CF(1.5,0.5+.08*SIN(SINE/35),-0.1)*ANGLES(RAD(-8.5),RAD(15.2+2*COS(SINE/35)),RAD(14.5+6*COS(SINE/35))),Alpha)
  		Neck.C0 = Neck.C0:lerp(NECKC0*CF(0,0,0+ ((1) - 1))*CF(0,0,0)*ANGLES(RAD(8.5+3.55*SIN(SINE/35)),RAD(0),RAD(0)),Alpha)
		end
	elseif TORSOVELOCITY > 1 and HITFLOOR ~= nil then
		ANIM = "Walk"
		if ATTACK == false then
		local Alpha = .1
  		RootJoint.C0 = RootJoint.C0:lerp(CF(0,0+.75*COS(SINE/35),0)*ANGLES(RAD(0+RAD(TiltVelocity.Z*50)),RAD(0+RAD(TiltVelocity.X*15)),RAD(0-RAD(TiltVelocity.X*15)))*ROOTC0,Alpha)
  		lh.C0 = lh.C0:lerp(CF(-1.1,-0.8-.06*SIN(SINE/35),-0.9)*ANGLES(RAD(-4.9),RAD(2.3),RAD(102.2)),Alpha)
  		rh.C0 = rh.C0:lerp(CF(0.5,-0.7-.06*SIN(SINE/35),-0.5)*ANGLES(RAD(0.2+2*COS(SINE/35)+RAD(TiltVelocity.Z)*25),RAD(-3.8),RAD(1.9+1.65*SIN(SINE/35)-RAD(TiltVelocity.X*15))),Alpha)
  		la.C0 = la.C0:lerp(CF(-1.5,0.5+.08*SIN(SINE/35),-0.1)*ANGLES(RAD(-9.5),RAD(-18.6-2*COS(SINE/35)),RAD(-14.8-6*COS(SINE/35))),Alpha)
  		ra.C0 = ra.C0:lerp(CF(1.5,0.5+.08*SIN(SINE/35),-0.1)*ANGLES(RAD(-8.5),RAD(15.2+2*COS(SINE/35)),RAD(14.5+6*COS(SINE/35))),Alpha)
  		Neck.C0 = Neck.C0:lerp(NECKC0*CF(0,0,0+ ((1) - 1))*CF(0,0,0)*ANGLES(RAD(8.5+3.55*SIN(SINE/35)+RAD(TiltVelocity.Z*50)),RAD(0),RAD(0+RAD(TiltVelocity.X*15))),Alpha)
		end
	end
	bigring1weld.C1 = bigring1weld.C1:lerp(CF(0,0,0)*ANGLES(RAD(SINE*1.25),RAD(0),RAD(0)),.1)
	bigring2weld.C1 = bigring2weld.C1:lerp(CF(0,0,0)*ANGLES(RAD(-SINE),RAD(0),RAD(0)),.1)
	smallringweld.C1 = smallringweld.C1:lerp(CF(0,0,0)*ANGLES(RAD(-SINE*1.5),0,0),.1)
	mediumringweld.C1 = mediumringweld.C1:lerp(CF(0,0,0)*ANGLES(RAD(SINE*1.75),0,0),.1)
	wing1weld.C1 = wing1weld.C1:lerp(CF(0,-1.25+1*SIN(SINE/35),-1.25)*ANGLES(RAD(42.5+30*SIN(SINE/35)),0,0),.1)
	wing2weld.C1 = wing2weld.C1:lerp(CF(0,-1.25+1*SIN(SINE/35),-2.5)*ANGLES(RAD(59.5+30*SIN(SINE/35)),0,0),.1)
	wing3weld.C1 = wing3weld.C1:lerp(CF(0,-.75+1*SIN(SINE/35),-4)*ANGLES(RAD(78.5+30*SIN(SINE/35)),0,0),.1)
	wing4weld.C1 = wing4weld.C1:lerp(CF(0,-1.25+1*SIN(SINE/35),1.25)*ANGLES(RAD(-42.5-30*SIN(SINE/35)),0,0),.1)
	wing5weld.C1 = wing5weld.C1:lerp(CF(0,-1.25+1*SIN(SINE/35),2.5)*ANGLES(RAD(-59.5-30*SIN(SINE/35)),0,0),.1)
	wing6weld.C1 = wing6weld.C1:lerp(CF(0,-.75+1*SIN(SINE/35),4)*ANGLES(RAD(-78.5-30*SIN(SINE/35)),0,0),.1)
	unanchor()
	if MRANDOM(1,15) == 1 then
	local pos = CF(HITPOS)*ANGLES(RAD(0),RAD(0),RAD(0))*CF(MRANDOM(-17,17)*1.5,0,MRANDOM(-17,17)*1.5).p
	local sfl,spos = Raycast(pos+VT(0,1,0), (CF(pos, pos + VT(0, -1, 0))).lookVector, 50, Character)
	local workffs = MRANDOM(-15,15)
	WACKYEFFECT2({Time = MRANDOM(75,95), EffectType = "Sphere", Size = VT(1,15,1), Size2 = VT(1,17,1), Transparency = 0, Transparency2 = 1, CFrame = CF(spos)*ANGLES(0,0,RAD(workffs)), MoveToPos = spos+VT(-workffs,MRANDOM(8,14)*4,0), RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
	end
	Humanoid.MaxHealth = 1e4
	Humanoid.Health = 1e4
	if Rooted == false then
		Disable_Jump = false
		Humanoid.WalkSpeed = Speed
	elseif Rooted == true then
		Disable_Jump = true
		Humanoid.WalkSpeed = 0
	end
	sick.SoundId = "rbxassetid://"..SONG
	sick.Looped = true
	sick.Pitch = 1
	sick.Volume = 6
	sick.Playing = true
end
