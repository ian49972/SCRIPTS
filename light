function rlrda()
	ATTACK = true
	local don = false

	for i = 0,20 do
		Swait()
		  	local Alpha = .1
  	RootJoint.C0 = RootJoint.C0:lerp(CF(0,0,0)*ANGLES(RAD(0),RAD(62),RAD(0))*ROOTC0,Alpha)
  	lh.C0 = lh.C0:lerp(CF(-1.1,-0.8,-0.9)*ANGLES(RAD(-4.9),RAD(2.3),RAD(102.2)),Alpha)
  	rh.C0 = rh.C0:lerp(CF(0.5,-0.7,-0.5)*ANGLES(RAD(0.2),RAD(-3.8),RAD(1.9)),Alpha)
  	la.C0 = la.C0:lerp(CF(-1.5,0.5,-0.1)*ANGLES(RAD(-9.5),RAD(-18.6),RAD(-14.8)),Alpha)
  	ra.C0 = ra.C0:lerp(CF(1.6,0.5,-0.1)*ANGLES(RAD(75.5),RAD(16.5),RAD(51.9)),Alpha)
  	Neck.C0 = Neck.C0:lerp(CF(0,0,0+ ((1) - 1))*CF(-0.2,0,0.1)*ANGLES(RAD(-17.7),RAD(-60.8),RAD(-15.5))*NECKC0,Alpha)
	end
	local portl = CreatePart(3,Effects,"Neon",0,0,"White","ssd",VT(.1,.1,.1),true)
	portl.Color = cols[2]
	MakeForm(portl,"Ball")
	portl.CFrame = RightArm.CFrame*CF(0,-4,0)
	portl.Orientation = VT(0,RootPart.Orientation.Y,0)
	CreateSound(233856106,portl,5,1,false)
	SetTween(portl,{Color = cols[1],CFrame = portl.CFrame*ANGLES(RAD(-20),0,0),Size = VT(10,5,.5)*2.5},"Sine","InOut",.7)
	delay(.7,function()
	WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = portl.Size, Size2 = portl.Size*2, Transparency = 0, Transparency2 = 1, CFrame = portl.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = portl.Color,SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})	
	end)
	delay(3,function()
	don = true
	end)
	local poop = 0
	coroutine.resume(coroutine.create(function()
			swait(.7)
	repeat
		Swait()
		poop = poop + 1 if poop >= 5 then poop = 0 end
		if poop == 1 then
		local proj = CreatePart(3,Effects,"Neon",0,0,"White","p",VT(1,1,1)*2,true)
		MakeForm(proj,"Ball")
		proj.Color = cols[1]
		proj.CFrame = portl.CFrame*CF(MRANDOM(-25,25)/2,MRANDOM(-12,12)/2,0)
		coroutine.resume(coroutine.create(function()
		for i = 1,50 do
			Swait()
			WACKYEFFECT2({Time = 15, EffectType = "Box", Size = VT(1.5,1.5,1.5), Size2 = VT(.5,.5,.5)*10, Transparency = 0, Transparency2 = 1, CFrame = proj.CFrame*CF(MRANDOM(-2,2),MRANDOM(-2,2),MRANDOM(-2,2)), MoveToPos = nil, RotationX = MRANDOM(-8,8), RotationY = MRANDOM(-8,8), RotationZ = MRANDOM(-8,8), Material = "Neon", Color = C3(1,1,1),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
			local hit,pos = Raycast(proj.Position,proj.CFrame.lookVector,500,Character)
			FireArc(proj,pos,MRANDOM(13,24),MRANDOM(20,30),false)
			if hit then break end
		end
		ApplyAoE(proj.Position,14,0,0,0,true)
		WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(30,30,30), Transparency = 0, Transparency2 = 1, CFrame = proj.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = 200633837, SoundPitch = 2, SoundVolume = 5, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
		for i = 1,5 do
			WACKYEFFECT2({Time = 60, EffectType = "Sphere", Size = VT(1,1,1), Size2 = VT(1,50,1), Transparency = 0, Transparency2 = 1, CFrame = proj.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,1,1),SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 15})
		end
		for i = 1,2 do
		WACKYEFFECT2({Time = 60, EffectType = "Box", Size = VT(1,1,1), Size2 = VT(30,30,30)/2, Transparency = 0, Transparency2 = 1, CFrame = proj.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = MRANDOM(-17,17), RotationY = MRANDOM(-17,17), RotationZ = MRANDOM(-17,17), Material = "Neon", Color = cols[1],SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 5})
		end
		--for i = 1,3 do
