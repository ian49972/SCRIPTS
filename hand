	for i = 0, .15, 0.1 / Animation_Speed do
			Swait()
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 10 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(90), RAD(-40), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
