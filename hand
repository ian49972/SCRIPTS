function animatefingers(option,tabl,alpha,easing,dir)
	if option == "tween" then
		Tween(bottomindex,{C0 = tabl[1]},easing,dir,alpha)
		Tween(topindex,{C0 = tabl[2]},easing,dir,alpha)
		Tween(bottommiddle,{C0 = tabl[3]},easing,dir,alpha)
		Tween(topmiddle,{C0 = tabl[4]},easing,dir,alpha)
		Tween(ringbottom,{C0 = tabl[5]},easing,dir,alpha)
		Tween(ringtop,{C0 = tabl[6]},easing,dir,alpha)
		Tween(bottompinkie,{C0 = tabl[7]},easing,dir,alpha)
		Tween(toppinkie,{C0 = tabl[8]},easing,dir,alpha)
		Tween(bottomthumb,{C0 = tabl[9]},easing,dir,alpha)
		Tween(topthumb,{C0 = tabl[10]},easing,dir,alpha)
	elseif option == "lerp" and not easing and not dir then
		bottomindex.C0 = bottomindex.C0:lerp(tabl[1],alpha)	
		topindex.C0 = topindex.C0:lerp(tabl[2],alpha)	
		bottommiddle.C0 = bottommiddle.C0:lerp(tabl[3],alpha)	
		topmiddle.C0 = topmiddle.C0:lerp(tabl[4],alpha)	
		ringbottom.C0 = ringbottom.C0:lerp(tabl[5],alpha)	
		ringtop.C0 = ringtop.C0:lerp(tabl[6],alpha)	
		bottompinkie.C0 = bottompinkie.C0:lerp(tabl[7],alpha)	
		toppinkie.C0 = toppinkie.C0:lerp(tabl[8],alpha)	
		bottomthumb.C0 = bottomthumb.C0:lerp(tabl[9],alpha)	
		topthumb.C0 = topthumb.C0:lerp(tabl[10],alpha)	
	end
end

