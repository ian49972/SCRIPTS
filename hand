				WACKYEFFECT({Time = 110, EffectType = "Wave", Size = VT(15,2,15) * 4, Size2 = VT(60,2,60) * 4, Transparency = 0.4, Transparency2 = 1, CFrame = CF(POS) * ANGLES(RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360))), MoveToPos = nil, RotationX =  RAD(MRANDOM(-360, 360)), RotationY = RAD(MRANDOM(-360, 360)), RotationZ = RAD(MRANDOM(-360, 360)), Material = "Neon", Color = C3(.3,1,.3), SoundID = nil, SoundPitch = 1, SoundVolume = 10, UseBoomerangMath = true, Boomerang = 10, SizeBoomerang = 20})
				WACKYEFFECT({Time = 110, EffectType = "Wave", Size = VT(15,2,15) * 4, Size2 = VT(60,2,60) * 4, Transparency = 0.4, Transparency2 = 1, CFrame = CF(POS) * ANGLES(RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360))), MoveToPos = nil, RotationX =  RAD(MRANDOM(-360, 360)), RotationY = RAD(MRANDOM(-360, 360)), RotationZ = RAD(MRANDOM(-360, 360)), Material = "Neon", Color = C3(.3,1,.3), SoundID = nil, SoundPitch = 1, SoundVolume = 10, UseBoomerangMath = true, Boomerang = 10, SizeBoomerang = 20})
				WACKYEFFECT({Time = 110, EffectType = "Wave", Size = VT(15,2,15)*2, Size2 = VT(60,2,60)*2, Transparency = 0, Transparency2 = 1, CFrame = CF(POS) * ANGLES(RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360))), MoveToPos = nil, RotationX =  RAD(MRANDOM(-360, 360)), RotationY = RAD(MRANDOM(-360, 360)), RotationZ = RAD(MRANDOM(-360, 360)), Material = "Neon", Color = C3(.2,1,.2), SoundID = nil, SoundPitch = 1, SoundVolume = 10, UseBoomerangMath = true, Boomerang = 10, SizeBoomerang = 20})
				WACKYEFFECT({Time = 110, EffectType = "Wave", Size = VT(15,2,15)*2, Size2 = VT(60,2,60)*2, Transparency = 0, Transparency2 = 1, CFrame = CF(POS) * ANGLES(RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360))), MoveToPos = nil, RotationX =  RAD(MRANDOM(-360, 360)), RotationY = RAD(MRANDOM(-360, 360)), RotationZ = RAD(MRANDOM(-360, 360)), Material = "Neon", Color = C3(.2,1,.2), SoundID = nil, SoundPitch = 1, SoundVolume = 10, UseBoomerangMath = true, Boomerang = 10, SizeBoomerang = 20})
				AOETime(RAY2.Position,145)
				RAY2:remove()
				break
			else
				RAY2.CFrame = Clerp(RAY2.CFrame,CF(RAY2.Position,Mouse.Hit.p),0.001) * CF(0,0,-4) 
				AOETime(RAY2.Position,35)
				WACKYEFFECT({
							Time = 12,
							EffectType = "Sphere",
							Size = VT(0, 0, 0),
							Size2 = VT(15, 10, 15)*2,
							Transparency = 0,
							Transparency2 = 1,
							CFrame = RAY2.CFrame * CF(0, 0, 0) * CFrame.new(0,-2.35,0) * ANGLES(RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)), RAD(SINE * 20)),
							MoveToPos = nil,
							RotationX = 0,
							RotationY = 0,
							RotationZ = 0,
							Material = "Neon",
							Color = C3(.4, .8, .4),
							SoundID = nil,
							SoundPitch = 2,
							SoundVolume = 10,
							UseBoomerangMath = true,
							Boomerang = 0,
							SizeBoomerang = 10
				})
			end	
		end   
		RAY2:remove()          	
	end))
	for i = 0, .5, 0.1 / Animation_Speed do
			Swait()
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(275), RAD(-10), RAD(90)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
			animatefingers("lerp",
			{
			--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(0)),
			--[[Top Index]] CF(0,5.5,0)*ANGLES(RAD(-50), RAD(0), RAD(0)),
			--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(0)),
			--[[Top Middle]] CF(0,5.5,0)*ANGLES(RAD(-50), RAD(0), RAD(0)),
			--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(85)),
			--[[Top Ring]] CF(0,-5,3.5)*ANGLES(RAD(85), RAD(0), RAD(0)),
			--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(85)),
			--[[Top Pinkie]] CF(0,-5,3.5)*ANGLES(RAD(85), RAD(0), RAD(0)),
			--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			--[[Top Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			}, .1)
			GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
	end
	GYRO:remove()
	ATTACK = false
end

function PunishingSmash()
	ATTACK = true
	for i = 0, 1, 0.1 / Animation_Speed do
			Swait()
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 30 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(90), RAD(-20), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
			animatefingers("lerp",
			{
			--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(0)),
			--[[Top Index]] CF(0,0,0)*ANGLES(RAD(-75), RAD(0), RAD(0)),
