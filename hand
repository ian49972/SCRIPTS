	Trail.Attachment1 = B
	Trail.Lifetime = 0.2
	Trail.LightEmission = 1
	Trail.Color = ColorSequence.new(BRICKC"Dark green".Color)
	Trail.Transparency = NumberSequence.new(0, 1)
	RAY.CFrame = CF(TI.TI.FingerModel.ShootPart.Position,Mouse.Hit.p) * CF(0, 0, 0) * ANGLES(RAD(0),RAD(0),RAD(180))  
	local RAY2 = CreatePart(3, Effects, "Neon", 0, 0, BRICKC("Dark green"), "Bullet", VT(5.5,1.5,5.5), true)
