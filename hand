local TweenService = game:GetService("TweenService")

function Tween(obj,prop,easing,easingdir,timer)
local easin = Enum.EasingStyle[easing]
local easindir = Enum.EasingDirection[easingdir]
local tweeninf = TweenInfo.new(
	timer/1,	
	easin,
	easindir,
	0,
	false,
	0
)
local props = prop
local tweenanim = TweenService:Create(obj,tweeninf,props)
tweenanim:Play()
end

function weld(parent, part0, part1, c0, c1)
	local w = IT("Weld")
	w.Part0 = part0
	w.Part1 = part1
	w.C0 = c0
	w.C1 = c1
	w.Parent = parent
	return w
end

d = CF(0,0,0)

local hand = script["The Hand"]
for _,v in pairs(hand:GetDescendants()) do if v:IsA("BasePart") then v.Anchored = false v.Massless = true end end
local bottomindex = hand.IndexParts.BottomIndex.joint2.mw
local topindex = hand.IndexParts.TopIndex.joint2.mw
local bottommiddle = hand.MiddleParts.BottomMid.joint2.mw
local topmiddle = hand.MiddleParts.TopMid.joint2.mw
local bottompinkie = hand.PinkieParts.BottomPinkie.joint2.mw
local toppinkie = hand.PinkieParts.TopPinkie.joint2.mw
local ringbottom = hand.RingParts.BottomRing.joint2.mw
local ringtop = hand.RingParts.TopRing.joint2.mw
local bottomthumb = hand.ThumbParts.BottomThumb.joint2.mw
local topthumb = hand.ThumbParts.TopThumb.joint2.mw 
local handw = weld(Torso,hand.BaseHandParts.BaseHand.BH,Torso,d*ANGLES(RAD(90),RAD(0),RAD(-90)),d)

