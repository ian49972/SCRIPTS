end

function BirdFlip()
	ATTACK = true 
	for i = 0, 5.25, 0.1 / Animation_Speed do
			Swait()
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(0), RAD(0), RAD(180)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
			animatefingers("lerp",
			{
			--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(85)),
			--[[Top Index]] CF(0,-5,3.5)*ANGLES(RAD(85), RAD(0), RAD(0)),
			--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			--[[Top Middle]] CF(0,0,0)*ANGLES(RAD(-15), RAD(0), RAD(0)),
			--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(85)),
			--[[Top Ring]] CF(0,-5,3.5)*ANGLES(RAD(85), RAD(0), RAD(0)),
			--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(85)),
			--[[Top Pinkie]] CF(0,-5,3.5)*ANGLES(RAD(85), RAD(0), RAD(0)),
			--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(35), RAD(0), RAD(85)),
			--[[Top Thumb]] CF(0,0,-1.5)*ANGLES(RAD(35), RAD(0), RAD(85)),
			}, .1)
	end
	ATTACK = false
end

function FingerGun()
	ATTACK = true
	local GYRO = IT("BodyGyro", RootPart)
	GYRO.D = 15
	GYRO.P = 2000
	GYRO.MaxTorque = VT(0, 4000000, 0)
	for i = 0, 2.25, 0.1 / Animation_Speed do
			Swait()
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, -2 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(90), RAD(-10), RAD(90)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
			animatefingers("lerp",
			{
			--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(0)),
			--[[Top Index]] CF(0,5.5,0)*ANGLES(RAD(-50), RAD(0), RAD(0)),
