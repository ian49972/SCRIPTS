				})
	AOETime(RootPart.Position,55)
	for i=0, 2.5, 0.1 / Animation_Speed do
		Swait()
		AOETime(RootPart.Position,55)
			WACKYEFFECT({
							Time = 50,
							EffectType = "Sphere",
							Size = VT(10, 10, 10)*2,
							Size2 = VT(25, 25, 25)*2,
							Transparency = 0,
							Transparency2 = 1,
							CFrame = CF(POS) * CF(0, 0, 0) * CFrame.new(0,-2.35,0) * ANGLES(RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)), RAD(SINE * 20)),
							MoveToPos = nil,
							RotationX = 0,
							RotationY = 0,
							RotationZ = 0,
							Material = "Neon",
							Color = C3(.6, 1, .6),
							SoundID = nil,
							SoundPitch = 2,
							SoundVolume = 10,
							UseBoomerangMath = true,
							Boomerang = 0,
							SizeBoomerang = 10
				})
				WACKYEFFECT({
							Time = 50,
							EffectType = "Sphere",
							Size = VT(10, 10, 10)*3,
							Size2 = VT(25, 25, 25)*3,
							Transparency = .4,
							Transparency2 = 1,
							CFrame = CF(POS) * CF(0, 0, 0) * CFrame.new(0,-2.35,0) * ANGLES(RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)), RAD(SINE * 20)),
							MoveToPos = nil,
							RotationX = 0,
							RotationY = 0,
							RotationZ = 0,
							Material = "Neon",
							Color = C3(.5, 1, .5),
							SoundID = nil,
							SoundPitch = 2,
							SoundVolume = 10,
							UseBoomerangMath = true,
							Boomerang = 0,
							SizeBoomerang = 10
				})
				WACKYEFFECT({
							Time = 50,
							EffectType = "Sphere",
							Size = VT(10, 10, 10)*4,
							Size2 = VT(25, 25, 25)*4,
							Transparency = .6,
							Transparency2 = 1,
							CFrame = CF(POS) * CF(0, 0, 0) * CFrame.new(0,-2.35,0) * ANGLES(RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)), RAD(SINE * 20)),
							MoveToPos = nil,
							RotationX = 0,
							RotationY = 0,
							RotationZ = 0,
							Material = "Neon",
							Color = C3(.4, 1, .4),
							SoundID = nil,
							SoundPitch = 2,
							SoundVolume = 10,
							UseBoomerangMath = true,
							Boomerang = 0,
							SizeBoomerang = 10
				})
	end
	ATTACK = false
end

function FingerBeams()
	ATTACK = true
	local IndexBeamPart = TI.TI.FingerModel.LaserPart
	local MiddleBeamPart = TM.TM.Part1.LaserPart
	local PinkieBeamPart = TP.TP.FingerModel.LaserPart
	local RingBeamPart = TR.TR.FingerModel.LaserPart
	local ThumbBeamPart = TT.TT.FingerModel.LaserPart
	local GYRO = IT("BodyGyro", RootPart)
	GYRO.D = 15
	GYRO.P = 2000
	GYRO.MaxTorque = VT(0, 4000000, 0) 
	local LOOPINGBEAMSOUND = CreateSound("2572704956",STUFF,7.5,.95,true)
	coroutine.resume(coroutine.create(function()
		 repeat 
			Swait()
				GYRO.CFrame = CF(RootPart.Position,Mouse.Hit.p)
				--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 25 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(55), RAD(-10), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
				animatefingers("lerp",
				{
				--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(25 - 10 * COS(SINE/30)), RAD(45 - 15 * COS(SINE/30))),
				--[[Top Index]] CF(0,0,0)*ANGLES(RAD(-75 - 15 * COS(SINE/30)), RAD(25 - 10 * COS(SINE/30)), RAD(0)),
				--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(10 - 5 * COS(SINE/30)), RAD(45 - 15 * COS(SINE/30))),
				--[[Top Middle]] CF(0,0,0)*ANGLES(RAD(-75 - 15 * COS(SINE/30)), RAD(10 - 5 * COS(SINE/30)), RAD(0)),
				--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(-10 + 5 * COS(SINE/30)), RAD(45 - 15 * COS(SINE/30))),
				--[[Top Ring]] CF(0,0,0)*ANGLES(RAD(-75 - 15 * COS(SINE/30)), RAD(-10 + 5 * COS(SINE/30)), RAD(0)),
				--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(-25 + 10 * COS(SINE/30)), RAD(45 - 15 * COS(SINE/30))),
				--[[Top Pinkie]] CF(0,0,0)*ANGLES(RAD(-75 - 15 * COS(SINE/30)), RAD(-25 + 10 * COS(SINE/30)), RAD(0)),
				--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(10), RAD(15), RAD(0)),
				--[[Top Thumb]] CF(0,0,0)*ANGLES(RAD(10), RAD(15), RAD(0)),
				}, .1)	
		 until KEYHOLD == false
	end))
	repeat
		 Swait()
		 local HIT,POS = CastProperRay(RightArm.Position, Mouse.Hit.p, 1000, Character)
		 SpawnTrail(IndexBeamPart.Position,POS)
		 SpawnTrail(MiddleBeamPart.Position,POS)
		 SpawnTrail(PinkieBeamPart.Position,POS)
		 SpawnTrail(RingBeamPart.Position,POS)
		 SpawnTrail(ThumbBeamPart.Position,POS)	
		 WACKYEFFECT({
			Time = 3,
			EffectType = "Block",
			Size = VT(15, 15, 15)/1.5,
			Size2 = VT(60, 60, 60)/1.5,
			Transparency = 0,
			Transparency2 = 1,
			CFrame = CF(POS),
			MoveToPos = nil,
			RotationX = math.random(-15,15),
			RotationY = math.random(-15,15),
			RotationZ = math.random(-15,15),
			Material = "Neon",
			Color = C3(.45,1,.45),
			SoundID = 0,
			SoundPitch = 0,
			SoundVolume = 7,
			UseBoomerangMath = true,
			Boomerang = 10,
			SizeBoomerang = 20
		})
			AOETime(POS,35)
	until KEYHOLD == false
	LOOPINGBEAMSOUND:remove()
	GYRO:remove()
	ATTACK = false
end

function TheDestroyer()
	ATTACK = true
	local HOLD = true
	local DESTROYER = CreatePart(3, Effects, "Neon", 0, 0, "Olivine", "DESTROYER", VT(0, 0, 0), true)
	DESTROYER.Shape = "Ball"
	DESTROYER.Touched:Connect(function(HIT)
		if HIT.Parent ~= Character and HIT.Parent:FindFirstChildOfClass("Humanoid") then
			OBLITERATE(HIT.Parent)
		end
	end)
	local THROW = false  
	CreateSound("306948842",STUFF,5,.8,false)  
	CreateSound("306948842",STUFF,5,.7,false)  
	CreateSound("306948842",STUFF,5,.6,false)  
	CreateSound("306948842",STUFF,5,.5,false)  
	CreateSound("306948842",STUFF,5,.4,false)  
	CreateSound("306948842",STUFF,5,.3,false)  
	CreateSound("489657591",STUFF,5,7,false)
	coroutine.resume(coroutine.create(function()
			repeat
				DESTROYER.CFrame = Torso.CFrame * CF(0, 0, -55)
				Swait()
			until THROW == true
	end))
	coroutine.resume(coroutine.create(function()
		for E = 1, 180 do
			Swait()
			CameraShake(RootPart.Position,1e9,35,90)
			WACKYEFFECT({
				Time = 40,
				EffectType = "Sphere",
				Size = VT(5, 5, 5)*2.5,
				Size2 = VT(0, 0, 250)*2.5,
				Transparency = 0,
				Transparency2 = 1,
				CFrame = DESTROYER.CFrame * ANGLES(RAD(MRANDOM(-360, 360)), RAD(MRANDOM(-360, 360)), RAD(MRANDOM(-360, 360))),
				MoveToPos = nil,
				RotationX = math.random(-180,180),
				RotationY = math.random(-15,15),
				RotationZ = math.random(-65,65),
				Material = "Neon",
				Color = C3(.45,1,.45),
				SoundID = 0,
				SoundPitch = .8,
				SoundVolume = 4,   
				UseBoomerangMath = true,
				Boomerang = 25,
				SizeBoomerang = 50
			})
			DESTROYER.Size = DESTROYER.Size + VT(0.5, 0.5, 0.5)
			local ANGLE = CF(DESTROYER.Position) * ANGLES(RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)), RAD(MRANDOM(0, 360)))
		end
	end))
	for i=0, 5.5, 0.1 / Animation_Speed do
		Swait()
		--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 85 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(-80), RAD(-60), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		animatefingers("lerp",
		{
		--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Index]] CF(0,0,0)*ANGLES(RAD(35  - 25 * COS(SINE/30)), RAD(0), RAD(0)),
		--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Middle]] CF(0,0,0)*ANGLES(RAD(35  - 25 * COS(SINE/30)), RAD(0), RAD(0)),
		--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Ring]] CF(0,0,0)*ANGLES(RAD(35  - 25 * COS(SINE/30)), RAD(0), RAD(0)),
		--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Pinkie]] CF(0,0,0)*ANGLES(RAD(35  - 25 * COS(SINE/30)), RAD(0), RAD(0)),
		--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(35  - 25 * COS(SINE/30))),
		}, .1)
	end
	local GYRO = IT("BodyGyro", RootPart)
	GYRO.D = 100
	GYRO.P = 20000
	GYRO.MaxTorque = VT(0, 4000000, 0)
	GYRO.cframe = CF(RootPart.Position, Mouse.Hit.p)
	HOLD = false
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		GYRO.CFrame = CF(RootPart.Position,Mouse.Hit.p)
		--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 85 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(-80), RAD(-60), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		animatefingers("lerp",
		{
		--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Index]] CF(0,0,0)*ANGLES(RAD(35  - 25 * COS(SINE/30)), RAD(0), RAD(0)),
		--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Middle]] CF(0,0,0)*ANGLES(RAD(35  - 25 * COS(SINE/30)), RAD(0), RAD(0)),
		--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Ring]] CF(0,0,0)*ANGLES(RAD(35  - 25 * COS(SINE/30)), RAD(0), RAD(0)),
		--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Pinkie]] CF(0,0,0)*ANGLES(RAD(35  - 25 * COS(SINE/30)), RAD(0), RAD(0)),
		--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25  - 25 * COS(SINE/30))),
		--[[Top Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(35  - 25 * COS(SINE/30))),
		}, .1)
	end
	CameraShake(DESTROYER.Position,1e9,45,105)
	CreateSound("533636431",STUFF,10,1,false)
	CreateSound("533636431",STUFF,10,.9,false)
	CreateSound("533636431",STUFF,10,.8,false)
	for i=0, .5, 0.1 / Animation_Speed do
		Swait()
		GYRO.CFrame = CF(RootPart.Position,Mouse.Hit.p)
		--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 85 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(120), RAD(40), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
		animatefingers("lerp",
		{
		--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25)),
		--[[Top Index]] CF(0,0,0)*ANGLES(RAD(35), RAD(0), RAD(0)),
		--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25)),
		--[[Top Middle]] CF(0,0,0)*ANGLES(RAD(35), RAD(0), RAD(0)),
		--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25)),
		--[[Top Ring]] CF(0,0,0)*ANGLES(RAD(35), RAD(0), RAD(0)),
		--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25)),
		--[[Top Pinkie]] CF(0,0,0)*ANGLES(RAD(35), RAD(0), RAD(0)),
		--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(25)),
		--[[Top Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(35)),
		}, .1)
	end
	DESTROYER.CFrame = CF(Torso.Position, Mouse.Hit.p)
	THROW = true
	GYRO:remove()
	coroutine.resume(coroutine.create(function()
		for i = 1, 1000 do
			Swait()
			local HIT,POS,NORMAL = Raycast(DESTROYER.Position,DESTROYER.CFrame.lookVector,4,Character)
			if HIT then
				CameraShake(DESTROYER.Position,1e9,50,125)
				CreateSound("541452480",STUFF,5,1,false)
				CreateSound("1606720250",STUFF,10,1,false)
				CreateSound("1606720250",STUFF,10,.9,false)
				CreateSound("1606720250",STUFF,10,.8,false)
				CreateSound("1606719431",STUFF,10,1,false)
				CreateSound("1606719431",STUFF,10,.9,false)
				CreateSound("1606719431",STUFF,10,.8,false)
				CreateSound("421328847",STUFF,10,1,false)
				CreateSound("421328847",STUFF,10,.9,false)
				CreateSound("421328847",STUFF,10,.8,false)
				CreateSound("1606720250",STUFF,10,1,false)
				CreateSound("1606720250",STUFF,10,.9,false)
				CreateSound("1606720250",STUFF,10,.8,false)
				CreateSound("1606719431",STUFF,10,1,false)
				CreateSound("1606719431",STUFF,10,.9,false)
				CreateSound("1606719431",STUFF,10,.8,false)
				CreateSound("421328847",STUFF,10,1,false)
				CreateSound("421328847",STUFF,10,.9,false)
				CreateSound("421328847",STUFF,10,.8,false)
				coroutine.resume(coroutine.create(function()
					for i = 1,250 do
						Swait()
						WACKYEFFECT({EffectType = "Wave", Size = VT(0,5,0), Size2 = VT(i*12,5,i*12), Transparency = 0.6, Transparency2 = 1, CFrame = CF(POS) * ANGLES(RAD(0), RAD(MRANDOM(0,360)), RAD(MRANDOM(-5,5))), MoveToPos = nil, RotationX = 0.1, RotationY = 1, RotationZ = -0.1, Material = "Slate", Color = C3(.45,1,.45), SoundID = 0, SoundPitch = MRANDOM(5,15)/10, SoundVolume = 3, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
						WACKYEFFECT({EffectType = "Sphere", Size = VT(0,5,0), Size2 = VT(i*12,5,i*12), Transparency = 0.6, Transparency2 = 1, CFrame = CF(POS) * ANGLES(RAD(0), RAD(MRANDOM(0,360)), RAD(MRANDOM(-5,5))), MoveToPos = nil, RotationX = 0.1, RotationY = 1, RotationZ = -0.1, Material = "Neon", Color = C3(.45,1,.45), SoundID = 0, SoundPitch = MRANDOM(5,15)/10, SoundVolume = 3, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
						WACKYEFFECT({EffectType = "Slash", Size = VT(0,5,0), Size2 = VT(i*12,5,i*12), Transparency = 0.6, Transparency2 = 1, CFrame = CF(POS) * ANGLES(RAD(0), RAD(MRANDOM(0,360)), RAD(MRANDOM(-5,5))), MoveToPos = nil, RotationX = 0.1, RotationY = 1, RotationZ = -0.1, Material = "Neon", Color = C3(.45,1,.45), SoundID = 0, SoundPitch = MRANDOM(5,15)/10, SoundVolume = 3, UseBoomerangMath = false, Boomerang = 0, SizeBoomerang = 0})
					end
				end))
				AOETime2(POS,15e15)
				DESTROYER:remove()
				break		
			else
				DESTROYER.CFrame = Clerp(DESTROYER.CFrame,CF(DESTROYER.Position,Mouse.Hit.p),0.001) * CF(0,0,-2) 
				WACKYEFFECT({
					Time = 40,
					EffectType = "Sphere",
					Size = VT(5, 5, 5)*2.5,
					Size2 = VT(0, 0, 250)*2.5,
					Transparency = 0,
					Transparency2 = 1,
					CFrame = DESTROYER.CFrame * ANGLES(RAD(MRANDOM(-360, 360)), RAD(MRANDOM(-360, 360)), RAD(MRANDOM(-360, 360))),
					MoveToPos = nil,
					RotationX = math.random(-180,180),
					RotationY = math.random(-15,15),
					RotationZ = math.random(-65,65),
					Material = "Neon",
					Color = C3(.45,1,.45),
					SoundID = 0,
					SoundPitch = .8,
					SoundVolume = 4,   
					UseBoomerangMath = true,
					Boomerang = 25,
					SizeBoomerang = 50
				})
			end
		end 
	end))                 
	ATTACK = false
end

--><Key Inputs

function MouseDown(Mouse)       
	KEYHOLD = true
	if ATTACK == false then

	end           
end

function MouseUp(Mouse)
HOLD = false
end

function KeyDown(Key)
	KEYHOLD = true
	if Key == "z" and ATTACK == false then
		FingerGun()
	end
	if Key == "x" and ATTACK == false then
		TheDestroyer()
	end
	if Key == "c" and ATTACK == false then
		TimeDispeller()
	end
	if Key == "v" and ATTACK == false then
		PunishingSmash()
	end
	if Key == "b" and ATTACK == false then
		FingerBeams()
	end
	if Key == "t" and ATTACK == false then
		Taunts()
	end
	if Key == "f" and ATTACK == false then
		BirdFlip()
	end
	if Key == "m" and ATTACK == false then
		if Song == 2803669496 then
			Song = 0
			print("Song Off")
		elseif Song == 0 then
			Song = 2803669496
			print("Song On")
		end	             				
	end
end	

function KeyUp(Key)
	KEYHOLD = false
end

	Mouse.Button1Down:connect(function(NEWKEY)
		MouseDown(NEWKEY)
	end)
	Mouse.Button1Up:connect(function(NEWKEY)
		MouseUp(NEWKEY)
	end)
	Mouse.KeyDown:connect(function(NEWKEY)
		KeyDown(NEWKEY)
	end)
	Mouse.KeyUp:connect(function(NEWKEY)
		KeyUp(NEWKEY)
	end) 

--><
function unanchor()
	for _, c in pairs(Character:GetChildren()) do
		if c:IsA("BasePart") and c ~= RootPart then
			c.Anchored = false
		end
	end
	if UNANCHOR == true then
		RootPart.Anchored = false
	else
		RootPart.Anchored = true
	end
end

------------------------------------------------------------------------------
Humanoid.Changed:connect(function(Jump)
	if Jump == "Jump" and (Disable_Jump == true) then
		Humanoid.Jump = false
	end
end)

Humanoid.HipHeight = 25
Humanoid.JumpPower = 0
Humanoid.WalkSpeed = 100
local Field = Instance.new("ForceField", game.Players.LocalPlayer.Character)
Field.Visible = false
Character.Parent = workspace

while true do
	Swait()
	script.Parent = WEAPONGUI
	ANIMATE.Parent = nil
	ANIMATOR.Parent = nil
	for _,v in next, Humanoid:GetPlayingAnimationTracks() do
	    v:Stop();
	end
	SINE = SINE + CHANGE*2
	local TORSOVELOCITY = (RootPart.Velocity * VT(1, 0, 1)).magnitude
	local TORSOVERTICALVELOCITY = RootPart.Velocity.y
	local HITFLOOR,HITPOS,NORMAL = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 4+Humanoid.HipHeight, Character)
	local TiltVelocity = CF(RootPart.CFrame:vectorToObjectSpace(RootPart.Velocity/1.6))
	local WALKSPEEDVALUE = 12 / (Humanoid.WalkSpeed / 16)
	if TORSOVELOCITY < 1 and HITFLOOR ~= nil then
		ANIM = "Idle"
		if ATTACK == false then
			local Alpha = .1
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0 + 3.5 * COS(SINE / 44)) * ANGLES(RAD(0 - 5.5 * SIN(SINE / 45)), RAD(-10), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
			animatefingers("lerp",
			{
			--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(15 - 25 * COS(SINE/30))),
			--[[Top Index]] CF(0,0,0)*ANGLES(RAD(15 - 25 * COS(SINE/30)), RAD(0), RAD(0)),
			--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(15 - 25 * COS(SINE/30))),
			--[[Top Middle]] CF(0,0,0)*ANGLES(RAD(15 - 25 * COS(SINE/30)), RAD(0), RAD(0)),
			--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(15 - 25 * COS(SINE/30))),
			--[[Top Ring]] CF(0,0,0)*ANGLES(RAD(15 - 25 * COS(SINE/30)), RAD(0), RAD(0)),
			--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(15 - 25 * COS(SINE/30))),
			--[[Top Pinkie]] CF(0,0,0)*ANGLES(RAD(15 - 25 * COS(SINE/30)), RAD(0), RAD(0)),
			--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(15 - 25 * COS(SINE/30))),
			--[[Top Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(15 - 25 * COS(SINE/30))),
			}, Alpha)
		end
	elseif TORSOVELOCITY > 1 and HITFLOOR ~= nil then
		ANIM = "Walk"
		if ATTACK == false then
			local Alpha = .1
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0 + 3.5 * COS(SINE / 44)) * ANGLES(RAD(45 + 6.5 * SIN(SINE / 45)), RAD(-10), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
			animatefingers("lerp",
			{
			--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(10), RAD(10), RAD(0)),
			--[[Top Index]] CF(0,0,0)*ANGLES(RAD(10), RAD(10), RAD(0)),
			--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(5), RAD(5), RAD(0)),
			--[[Top Middle]] CF(0,0,0)*ANGLES(RAD(5), RAD(5), RAD(0)),
			--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(-5), RAD(-5), RAD(0)),
			--[[Top Ring]] CF(0,0,0)*ANGLES(RAD(-5), RAD(-5), RAD(0)),
			--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(-10), RAD(-10), RAD(0)),
			--[[Top Pinkie]] CF(0,0,0)*ANGLES(RAD(-10), RAD(-10), RAD(0)),
			--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(10), RAD(15), RAD(0)),
			--[[Top Thumb]] CF(0,0,0)*ANGLES(RAD(10), RAD(15), RAD(0)),
			}, Alpha)
		end
	end                          
	unanchor() 
	Head.CanCollide = false
	Torso.CanCollide = false
	RightArm.CanCollide = false
	LeftArm.CanCollide = false
	RightLeg.CanCollide = false
	LeftLeg.CanCollide = false
	Humanoid.WalkSpeed = 100
	Humanoid.HipHeight = 25
	Humanoid.JumpPower = 0
	Humanoid:SetStateEnabled("Dead",false)
	Humanoid:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
	Humanoid:SetStateEnabled("GettingUp",true)
	Humanoid:SetStateEnabled(Enum.HumanoidStateType.GettingUp, true)
	Humanoid:SetStateEnabled("Ragdoll",false)
	Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
	Humanoid:SetStateEnabled("FallingDown",false)
	Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
	Humanoid:SetStateEnabled("Seated",false)
	Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
	Humanoid:SetStateEnabled("PlatformStanding",false)
	Humanoid:SetStateEnabled(Enum.HumanoidStateType.PlatformStanding, false)
	Humanoid.MaxHealth = 1e9999999
	Humanoid.Health = 1e999999999 
	Humanoid.Name = "Being"
	if game.ReplicatedFirst:FindFirstChildOfClass("Humanoid") == nil then
		Humanoid = IT("Humanoid",game.ReplicatedFirst)
        Humanoid.Name = "Being"
        Humanoid.HipHeight = 0
	end
	if Rooted == false then
		Disable_Jump = false
		Humanoid.WalkSpeed = Speed
	elseif Rooted == true then
		Disable_Jump = true
		Humanoid.WalkSpeed = 0
	end 	
	Character.Parent = workspace
	if game.ReplicatedFirst:FindFirstChildOfClass("Humanoid") == nil then
		Humanoid = IT("Humanoid",game.ReplicatedFirst)
        Humanoid.Name = "Being"
        Humanoid.HipHeight = 0
	end
	if Character:FindFirstChildOfClass("Accessory") then
		Character:FindFirstChildOfClass("Accessory"):remove()
	elseif Character:FindFirstChildOfClass("Hair") then
		Character:FindFirstChildOfClass("Hair"):remove()
	end
	if Character:FindFirstChildOfClass("Accessory") then
		Character:FindFirstChildOfClass("Accessory"):remove()
	elseif Character:FindFirstChildOfClass("Hat") then
		Character:FindFirstChildOfClass("Hat"):remove()
	end
	if Player.Name == "InfiniteOneWithdank" then
		for _, c in pairs(Character:GetChildren()) do
			if c and c.Parent and c.ClassName == "ShirtGraphic" then
				c:Destroy()
			end
		end	
	end	
	Musika.SoundId = "rbxassetid://" .. Song
	Musika.Looped = true
	Musika.Pitch = Pitch
	Musika.Volume = Volu
	Musika.Playing = true
	Musika.EmitterSize = 35
	if Musika.Parent ~= RootPart then
		print("Fixing music")
		Musika = IT("Sound", RootPart)
		Musika.SoundId = "rbxassetid://" .. Song
		Musika.Looped = true
		Musika.Pitch = Pitch
		Musika.Volume = Volu 
		Musika.Playing = true
		Musika.EmitterSize = 45
		Musika.TimePosition = TSTP
		FIXING = true
	elseif FIXING == false then
		TSTP = Musika.TimePosition
	else
		FIXING = false
	end	
end
