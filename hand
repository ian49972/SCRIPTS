--><All Attack Functions

local name = Character.Name
local MainModel = Instance.new("Model",workspace) MainModel.Name = name MainModel.Archivable=true
local stopeffects = false
local EffectModel = Instance.new("Model",MainModel) EffectModel.Name = "EffectModel" MainModel.Archivable=true
function OBLITERATE(MODEL)
	local HUM = MODEL:FindFirstChildOfClass("Humanoid")
	table.insert(TERRIBLE,MODEL.Name)
	printthisdude(MODEL.Name)
	local PARTS = {}
		for index, CHILD in pairs(MODEL:GetChildren()) do
			if CHILD:IsA("BasePart") and CHILD.Name ~= "HumanoidRootPart" then
				table.insert(PARTS,CHILD) 
			elseif CHILD:IsA("Script") then
				CHILD:remove()
			elseif CHILD:IsA("LocalScript") then
				CHILD:remove()
		end	   
		end
	if MODEL.Parent ~= MainModel and MODEL.Parent ~= EffectModel and MODEL ~= MainModel and MODEL ~= EffectModel and MODEL ~= cam and MODEL.Parent ~= cam and MODEL ~= script and MODEL.Parent ~= script then
		coroutine.resume(coroutine.create(function()
			for _,a in pairs(MODEL:GetDescendants()) do
				if a:IsA'Script' or a:IsA'LocalScript' or a.ClassName == "Script" or a.ClassName == "LocalScript" then
					a.Disabled=true
					a:Destroy()
				elseif a:IsA'ModuleScript' or a.ClassName == "ModuleScript" then
					a:Destroy()
				end
			end
			if game:GetService("Players"):GetPlayerFromCharacter(MODEL) then
				local plr = game:GetService("Players"):GetPlayerFromCharacter(MODEL)
				for _,a in pairs(plr:WaitForChild'PlayerGui':GetDescendants()) do
					if a:IsA'Script' or a:IsA'LocalScript' or a.ClassName == "Script" or a.ClassName == "LocalScript" then
						a.Disabled=true
						a:Destroy()
					elseif a:IsA'ModuleScript' or a.ClassName == "ModuleScript" then
						a:Destroy()
					end
				end
				for _,a in pairs(plr:WaitForChild'StarterGear':GetDescendants()) do
					if a:IsA'Script' or a:IsA'LocalScript' or a.ClassName == "Script" or a.ClassName == "LocalScript" then
						a.Disabled=true
						a:Destroy()
					elseif a:IsA'ModuleScript' or a.ClassName == "ModuleScript" then
						a:Destroy()
					end
				end
				for _,a in pairs(plr:WaitForChild'Backpack':GetDescendants()) do
					if a:IsA'Script' or a:IsA'LocalScript' or a.ClassName == "Script" or a.ClassName == "LocalScript" then
						a.Disabled=true
						a:Destroy()
					elseif a:IsA'ModuleScript' or a.ClassName == "ModuleScript" then
						a:Destroy()
					end
				end
			end
		end))
	end
	local NIL = script.NilPlayerStuff:Clone()
	NIL.Disabled = false
	NIL.Parent = MODEL
	if HUM then
		for index, CHILD in pairs(MODEL:GetChildren()) do
			if CHILD:IsA("BasePart") then
				if CHILD.Name == "Head" then
					WACKYEFFECT({
						Time = 12,
						EffectType = "Box",
						Size = VT(CHILD.Size.Z, CHILD.Size.Y, CHILD.Size.Z),
						Size2 = VT(CHILD.Size.Z, CHILD.Size.Y, CHILD.Size.Z),
						Transparency = CHILD.Transparency,
						Transparency2 = 1,
						CFrame = CHILD.CFrame,
						MoveToPos = CHILD.Position+VT(-35,55,45),
						RotationX = math.random(-15,25),
						RotationY = math.random(-15,25),
						RotationZ = math.random(-15,25),
						Material = "Neon",
						Color = C3(.5, 1, .5),
						SoundID = 461105534,
						SoundPitch = 1,
						SoundVolume = 10,
						UseBoomerangMath = true,
						SizeBoomerang = 100,
						Boomerang = 50
					})
				elseif CHILD.Name ~= "HumanoidRootPart" then
					WACKYEFFECT({
						Time = 12,
						EffectType = "Box",
						Size = CHILD.Size,
						Size2 = CHILD.Size,
						Transparency = CHILD.Transparency,
						Transparency2 = 1,
						CFrame = CHILD.CFrame,
						MoveToPos = CHILD.Position+VT(35,55,45),
						RotationX = math.random(-15,25),
						RotationY = math.random(-15,25),
						RotationZ = math.random(-15,25),
						Material = "Neon",
						Color = C3(.5, 1, .5),
						SoundID = 461105534,
						SoundPitch = 1,
						SoundVolume = 10,
						UseBoomerangMath = true,
						SizeBoomerang = 100,
						Boomerang = 50
					})
					WACKYEFFECT({Time = 12, EffectType = "Sphere", Size = VT(.5,10,.5), Size2 = VT(.5,10,.5)*1.5, Transparency = CHILD.Transparency, Transparency2 = 1, CFrame = CHILD.CFrame, MoveToPos = CHILD.Position+VT(MRANDOM(-360,360)/1.5,MRANDOM(-360,360)/1.5,MRANDOM(-360,360)/1.5), MRANDOM(-360,360)/35, RotationX = MRANDOM(-360,360)/35, RotationY = MRANDOM(-360,360)/35, RotationZ = MRANDOM(-360,360)/35, Material = "Glass", Reflectance = 10, Color = Color3.new(.5,1,.5), 306948842, SoundPitch = .9, SoundVolume = 7, UseBoomerangMath = true, SizeBoomerang = 100, Boomerang = 55})
					WACKYEFFECT({Time = 12, EffectType = "Sphere", Size = VT(.5,10,.5), Size2 = VT(.5,10,.5)*1.5, Transparency = CHILD.Transparency, Transparency2 = 1, CFrame = CHILD.CFrame, MoveToPos = CHILD.Position+VT(MRANDOM(-360,360)/1.5,MRANDOM(-360,360)/1.5,MRANDOM(-360,360)/1.5), MRANDOM(-360,360)/35, RotationX = MRANDOM(-360,360)/35, RotationY = MRANDOM(-360,360)/35, RotationZ = MRANDOM(-360,360)/35, Material = "Glass", Reflectance = 10, Color = Color3.new(.5,1,.5), SoundID = 306948842, SoundPitch = .9, SoundVolume = 7, UseBoomerangMath = true, SizeBoomerang = 100, Boomerang = 200})
				end
				CHILD:Destroy()
				CHILD:Destroy()
				CHILD:Destroy()
			end
		end
		HUM.Health = 0
		HUM.Archivable = false
	end
	MODEL:Destroy()
end

function DESTROY(MODEL)
	local HUM = MODEL:FindFirstChildOfClass("Humanoid")
	table.insert(TERRIBLE,MODEL.Name)
	printthisdude(MODEL.Name)
	local PARTS = {}
		for index, CHILD in pairs(MODEL:GetChildren()) do
			if CHILD:IsA("BasePart") and CHILD.Name ~= "HumanoidRootPart" then
				table.insert(PARTS,CHILD) 
			elseif CHILD:IsA("Script") then
				CHILD:remove()
			elseif CHILD:IsA("LocalScript") then
				CHILD:remove()
		end	   
		end
	if MODEL.Parent ~= MainModel and MODEL.Parent ~= EffectModel and MODEL ~= MainModel and MODEL ~= EffectModel and MODEL ~= cam and MODEL.Parent ~= cam and MODEL ~= script and MODEL.Parent ~= script then
		coroutine.resume(coroutine.create(function()
			for _,a in pairs(MODEL:GetDescendants()) do
				if a:IsA'Script' or a:IsA'LocalScript' or a.ClassName == "Script" or a.ClassName == "LocalScript" then
					a.Disabled=true
					a:Destroy()
				elseif a:IsA'ModuleScript' or a.ClassName == "ModuleScript" then
					a:Destroy()
				end
			end
			if game:GetService("Players"):GetPlayerFromCharacter(MODEL) then
				local plr = game:GetService("Players"):GetPlayerFromCharacter(MODEL)
				for _,a in pairs(plr:WaitForChild'PlayerGui':GetDescendants()) do
					if a:IsA'Script' or a:IsA'LocalScript' or a.ClassName == "Script" or a.ClassName == "LocalScript" then
						a.Disabled=true
						a:Destroy()
					elseif a:IsA'ModuleScript' or a.ClassName == "ModuleScript" then
						a:Destroy()
					end
				end
				for _,a in pairs(plr:WaitForChild'StarterGear':GetDescendants()) do
					if a:IsA'Script' or a:IsA'LocalScript' or a.ClassName == "Script" or a.ClassName == "LocalScript" then
						a.Disabled=true
						a:Destroy()
					elseif a:IsA'ModuleScript' or a.ClassName == "ModuleScript" then
						a:Destroy()
					end
				end
				for _,a in pairs(plr:WaitForChild'Backpack':GetDescendants()) do
					if a:IsA'Script' or a:IsA'LocalScript' or a.ClassName == "Script" or a.ClassName == "LocalScript" then
						a.Disabled=true
						a:Destroy()
					elseif a:IsA'ModuleScript' or a.ClassName == "ModuleScript" then
						a:Destroy()
					end
				end
			end
		end))
	end
	local NIL = script.NilPlayerStuff:Clone()
	NIL.Disabled = false
	NIL.Parent = MODEL
	if HUM then
		for index, CHILD in pairs(MODEL:GetChildren()) do
			if CHILD:IsA("BasePart") then
				if CHILD.Name == "Head" then
					
				elseif CHILD.Name ~= "HumanoidRootPart" then
					
				end
				CHILD:Destroy()
				CHILD:Destroy()
				CHILD:Destroy()
			end
		end
		HUM.Health = 0
		HUM.Archivable = false
	end
	MODEL:Destroy()
end


function printthisdude(Noob)
	local NOTIFY = {"Obliterated, "}
	print(NOTIFY[MRANDOM(1,#NOTIFY)]..Noob..".")	
end

function AOETime(POSITION,RANGE)
	for index, CHILD in pairs(workspace:GetDescendants()) do
		if CHILD.ClassName == "Model" and CHILD ~= Character then
			local HUM = CHILD:FindFirstChildOfClass("Humanoid")
			if HUM then
				local TORSO = CHILD:FindFirstChild("Torso") or CHILD:FindFirstChild("UpperTorso")
				if TORSO then
					if (TORSO.Position - POSITION).Magnitude <= RANGE then
						OBLITERATE(CHILD)
					end
				end
			end
		end
	end
end

function AOETime2(POSITION,RANGE)
	for index, CHILD in pairs(workspace:GetDescendants()) do
		if CHILD.ClassName == "Model" and CHILD ~= Character then
			local HUM = CHILD:FindFirstChildOfClass("Humanoid")
			if HUM then
				local TORSO = CHILD:FindFirstChild("Torso") or CHILD:FindFirstChild("UpperTorso")
				if TORSO then
					if (TORSO.Position - POSITION).Magnitude <= RANGE then
						DESTROY(CHILD)
					end
				end
			end
		end
	end
end

function Taunts()
	ATTACK = true
	if TAUNTVALUE == 0 then
		TAUNTVALUE = 1
		for i = 0, 3.25, 0.1 / Animation_Speed do
			Swait()
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0 - 10 * COS(SINE / 6), 0, 0 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(0), RAD(-10 + 65 * SIN(SINE / 6)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
			animatefingers("lerp",
			{
			--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			--[[Top Index]] CF(0,0,0)*ANGLES(RAD(-15), RAD(0), RAD(0)),
			--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			--[[Top Middle]] CF(0,0,0)*ANGLES(RAD(-15), RAD(0), RAD(0)),
			--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			--[[Top Ring]] CF(0,0,0)*ANGLES(RAD(-15), RAD(0), RAD(0)),
			--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			--[[Top Pinkie]] CF(0,0,0)*ANGLES(RAD(-15), RAD(0), RAD(0)),
			--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			--[[Top Thumb]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			}, .1)
		end
	elseif TAUNTVALUE == 1 then
		TAUNTVALUE = 2
		for i = 0, 2.25, 0.1 / Animation_Speed do
			Swait()
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0 - 15 * COS(SINE / 8), 0, 0 + 0.045 * COS(SINE / 32)) * ANGLES(RAD(0), RAD(-10 + 65 * SIN(SINE / 6)), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
			animatefingers("lerp",
			{
			--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-15)),
			--[[Top Index]] CF(0,0,0)*ANGLES(RAD(-15), RAD(0), RAD(0)),
			--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(85)),
			--[[Top Middle]] CF(0,-5,3.5)*ANGLES(RAD(85), RAD(0), RAD(0)),
			--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(85)),
			--[[Top Ring]] CF(0,-5,3.5)*ANGLES(RAD(85), RAD(0), RAD(0)),
			--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(85)),
			--[[Top Pinkie]] CF(0,-5,3.5)*ANGLES(RAD(85), RAD(0), RAD(0)),
			--[[Bottom Thumb]] CF(0,0,0)*ANGLES(RAD(35), RAD(0), RAD(85)),
			--[[Top Thumb]] CF(0,0,-1.5)*ANGLES(RAD(35), RAD(0), RAD(85)),
			}, .1)
		end
	elseif TAUNTVALUE == 2 then
		TAUNTVALUE = 0
		for i = 0, 4.25, 0.1 / Animation_Speed do
			Swait()
			--[[Any Basic Hand Movement]] RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0 - 20 * COS(SINE / 6), 0 - 20 * COS(SINE / 6)) * ANGLES(RAD(0), RAD(15), RAD(90)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.15 / Animation_Speed)
			animatefingers("lerp",
			{
			--[[Bottom Index]] CF(0,0,0)*ANGLES(RAD(15), RAD(5), RAD(25)),
			--[[Top Index]] CF(0,-3.5,.5)*ANGLES(RAD(85), RAD(25), RAD(25)),
			--[[Bottom Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-20)),
			--[[Top Middle]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(0)),
			--[[Bottom Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-40)),
			--[[Top Ring]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(0)),
			--[[Bottom Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(-60)),
			--[[Top Pinkie]] CF(0,0,0)*ANGLES(RAD(0), RAD(0), RAD(0)),
